{"index.html":"<html><head><link href=\"https://cdn.syncfusion.com/ej2/28.1.33/{{theme}}.css\" rel=\"stylesheet\">\n\n    <link href=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css\" rel=\"stylesheet\">\n\n    <style>\n            body{\n                touch-action:none;\n            }\n            .control-section{\n                margin-top:100px;\n            }\n        </style></head><body><div class=\"stackblitz-container {{theme}}\">\n<div class=\"control-section\">\n    <div class=\"fab-appearence-container\">\n        <div class=\"row\">\n            <div class=\"col-xs-6 col-sm-6 col-lg-6 col-md-6\">\n                <div id=\"target1\" class=\"col-xs-12 col-sm-12 col-lg-6 col-md-6 fab-index\">\n                    <button id=\"fab1\" title=\"Primary\"></button><label class=\"fab-name\">Primary</label>\n                </div>\n                <div id=\"target2\" class=\"col-xs-12 col-sm-12 col-lg-6 col-md-6 fab-index\">\n                    <button id=\"fab2\" title=\"Secondary\"></button><label class=\"fab-name\">Secondary</label>\n                </div>\n            </div>\n            <div class=\"col-xs-6 col-sm-6 col-lg-6 col-md-6\">\n                <div id=\"target3\" class=\"col-xs-12 col-sm-12 col-lg-6 col-md-6 fab-index\">\n                    <button id=\"fab3\" title=\"Outline\"></button><label class=\"fab-name\">Outline</label>\n                </div>\n                <div id=\"target7\" class=\"col-xs-12 col-sm-12 col-lg-6 col-md-6 fab-index\">\n                    <button id=\"fab7\" title=\"Info\"></button><label class=\"fab-name\">Info</label>\n                </div>\n            </div>\n        </div>\n        <div class=\"row\">\n            <div class=\"col-xs-6 col-sm-6 col-lg-6 col-md-6\">\n                <div id=\"target4\" class=\"col-xs-12 col-sm-12 col-lg-6 col-md-6 fab-index\">\n                    <button id=\"fab4\" title=\"Success\"></button><label class=\"fab-name\">Success</label>\n                </div>\n                <div id=\"target5\" class=\"col-xs-12 col-sm-12 col-lg-6 col-md-6 fab-index\">\n                    <button id=\"fab5\" title=\"Warning\"></button><label class=\"fab-name\">Warning</label>\n                </div>\n            </div>\n            <div class=\"col-xs-6 col-sm-6 col-lg-6 col-md-6\">\n                <div id=\"target6\" class=\"col-xs-12 col-sm-12 col-lg-6 col-md-6 fab-index\">\n                    <button id=\"fab6\" title=\"Danger\"></button><label class=\"fab-name\">Danger</label>\n                </div>\n                <div id=\"target8\" class=\"col-xs-12 col-sm-12 col-lg-6 col-md-6 fab-index\">\n                    <button id=\"fab8\" title=\"Shopping\"></button><label class=\"fab-name\">Label shows on hover</label>\n                </div>\n            </div>\n        </div>\n    </div>\n</div>\n\n\n<style>\n    .fab-appearence-container {\n        width: 80%;\n        margin: 8% auto;\n        text-align: center;\n        max-width: 800px;\n    }\n\n    .fab-index{\n        z-index:500 !important;\n    }\n\n    .fab-appearence-container .col-xs-12 {\n        position: relative;\n        margin: 15px 0;\n        min-height: 100px;\n    }\n\n    .fab-appearence-container .col-md-6 {\n        padding: 0;\n    }\n\n    @media only screen and (max-width: 600px) {\n        .fab-appearence-container .col-xs-12 {\n            min-height: 110px;\n        }\n    }\n\n    /*start of hover css*/\n    .e-fab.e-btn.fab-hover {\n        padding: 0 10px;\n    }\n\n    .fab-hover .textEle {\n        margin-left: 5px;\n        float: right;\n    }\n\n    .fab-hover .text-container {\n        overflow: hidden;\n        width: 0;\n        margin: 0;\n        transition: width .5s linear 0s, margin .2s linear .5s;\n    }\n\n    .fab-hover:hover .text-container {\n        width: 70px;\n        margin: 0 5px;\n        transition: width .5s linear .2s, margin .2s linear 0s;\n    }\n\n    /*end of hover css*/\n    /*start of mobile view css*/\n    .e-bigger .e-btn.fab-hover .e-btn-icon.e-icon-left,\n    .e-btn.fab-hover .e-btn-icon.e-icon-left {\n        margin-left: 0;\n        width: 1em;\n    }\n\n    .e-bigger .fab-hover:hover .text-container {\n        width: 75px;\n    }\n    \n    body[class*=\"material3\"].e-bigger .fab-hover:hover .text-container,\n    body[class*=\"material3-dark\"].e-bigger .fab-hover:hover .text-container {\n        width: 65px;\n    }\n\n    .e-bigger .e-fab.e-btn.fab-hover {\n        padding: 0 13px;\n    }\n\n    .bootstrap4 .e-bigger .e-btn.fab-hover .e-btn-icon.e-icon-left,\n    .bootstrap4 .e-btn.fab-hover .e-btn-icon.e-icon-left {\n        padding: 0;\n    }\n    /*end of mobile view css*/\n\n    @font-face {\n        font-family: 'fab-icons';\n        src: url(data:application/x-font-ttf;charset=utf-8;base64,) format('truetype');\n        font-weight: normal;\n        font-style: normal;\n    }\n\n    [class^=\"fab-icon-\"],\n    [class*=\" fab-icon-\"] {\n        font-family: 'fab-icons' !important;\n        speak: none;\n        font-size: 55px;\n        font-style: normal;\n        font-weight: normal;\n        font-variant: normal;\n        text-transform: none;\n        line-height: inherit;\n        -webkit-font-smoothing: antialiased;\n        -moz-osx-font-smoothing: grayscale;\n    }\n\n    .fab-icon-shopping::before {\n        content: \"\\e70c\";\n    }\n\n    .e-fab.e-btn .e-btn-icon {\n        margin-top: 5px !important;\n    }\n</style>\n\n</div></body></html>","package.json":"{\n  \"@syncfusion/ej2-base\": \"*\",\n  \"markdown-spellcheck\": \"^1.3.1\",\n  \"@syncfusion/ej2-buttons\": \"*\",\n  \"@types/crossroads\": \"0.0.28\",\n  \"@types/signals\": \"0.0.16\",\n  \"@types/moment-timezone\": \"^0.5.3\",\n  \"@types/hasher\": \"0.0.27\",\n  \"fuse.js\": \"^3.2.0\",\n  \"marked\": \"5.1.2\",\n  \"codemirror\": \"^5.37.0\",\n  \"crossroads\": \"^0.12.2\",\n  \"@types/marked\": \"5.0.1\",\n  \"@types/codemirror\": \"0.0.56\",\n  \"@types/es6-promise\": \"0.0.28\",\n  \"hasher\": \"^1.2.0\",\n  \"moment-timezone\": \"^0.5.14\"\n}","index.ts":"{{ripple}}\r\nimport { Fab } from \"@syncfusion/ej2-buttons\";\r\n\r\n\r\n    \r\n\r\n    //Primary\r\n    let fabObj1: Fab = new Fab({\r\n        iconCss: 'fab-icons fab-icon-shopping',\r\n        position: 'BottomCenter',\r\n        target: '#target1',\r\n    })\r\n    fabObj1.appendTo('#fab1')\r\n\r\n    //Normal\r\n    let fabObj2: Fab = new Fab({\r\n        iconCss: 'fab-icons fab-icon-shopping',\r\n        isPrimary: false,\r\n        position: 'BottomCenter',\r\n        target: '#target2',\r\n    })\r\n    fabObj2.appendTo('#fab2')\r\n\r\n    //Outline\r\n    let fabObj3: Fab = new Fab({\r\n        iconCss: 'fab-icons fab-icon-shopping',\r\n        cssClass: 'e-outline',\r\n        position: 'BottomCenter',\r\n        target: '#target3',\r\n    })\r\n    fabObj3.appendTo('#fab3')\r\n\r\n    //Success\r\n    let fabObj4: Fab = new Fab({\r\n        iconCss: 'fab-icons fab-icon-shopping',\r\n        cssClass: 'e-success',\r\n        position: 'BottomCenter',\r\n        target: '#target4',\r\n    })\r\n    fabObj4.appendTo('#fab4')\r\n\r\n    //Warning\r\n    let fabObj5: Fab = new Fab({\r\n        iconCss: 'fab-icons fab-icon-shopping',\r\n        cssClass: 'e-warning',\r\n        position: 'BottomCenter',\r\n        target: '#target5',\r\n    })\r\n    fabObj5.appendTo('#fab5')\r\n\r\n    //Danger\r\n    let fabObj6: Fab = new Fab({\r\n        iconCss: 'fab-icons fab-icon-shopping',\r\n        cssClass: 'e-danger',\r\n        position: 'BottomCenter',\r\n        target: '#target6',\r\n    })\r\n    fabObj6.appendTo('#fab6')\r\n\r\n    //Info\r\n    let fabObj7: Fab = new Fab({\r\n        iconCss: 'fab-icons fab-icon-shopping',\r\n        cssClass: 'e-info',\r\n        position: 'BottomCenter',\r\n        target: '#target7',\r\n    })\r\n    fabObj7.appendTo('#fab7')\r\n\r\n    //On hover\r\n    let fabObj8: Fab = new Fab({\r\n        iconCss: 'fab-icons fab-icon-shopping',\r\n        position: 'BottomCenter',\r\n        content:'<span class=\"text-container\"><span class=\"textEle\">Shopping</span></span>',\r\n        cssClass: 'fab-hover',\r\n        target: '#target8',\r\n    })\r\n    fabObj8.appendTo('#fab8')\r\n\r\n"}