{"index.html":"<html><head><link href=\"https://cdn.syncfusion.com/ej2/28.1.33/{{theme}}.css\" rel=\"stylesheet\">\n\n    <link href=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css\" rel=\"stylesheet\">\n\n    <style>\n            body{\n                touch-action:none;\n            }\n            .control-section{\n                margin-top:100px;\n            }\n        </style></head><body><div class=\"stackblitz-container {{theme}}\">\n\n<div id=\"target\" class=\"control-section\">\n    <div class=\"pane1\">\n            <div id=\"horizontal-pane-heading\">Horizontal Splitter</div>\n        <div id=\"horizontal\">\n            <div class=\"splitter-default-content\">\n                <div>Left pane<div id=\"leftpanetext\">size: 25%</div><div id=\"leftpanetext1\">min: 60px</div></div>\n            </div>\n            <div class=\"splitter-default-content\">\n                <span>Middle pane<div id=\"middlepanetext\">size: 50%</div><div id=\"middlepanetext1\">min: 60px</div></span>\n            </div>\n            <div class=\"splitter-default-content\">\n                <span>Right pane<div id=\"rightpanetext\">size: 25%</div><div id=\"rightpanetext1\">min: 60px</div></span>\n            </div>\n        </div>\n    </div>\n    <div class=\"pane2\">\n            <div id=\"vertical-pane-heading\">Vertical Splitter</div>\n        <div id=\"vertical\">\n            <div class=\"splitter-default-content\">\n                <span>Top pane<div id=\"toppanetext\">size: 30%</div><div id=\"toppanetext1\">min: 60px</div></span>\n            </div>\n            <div class=\"splitter-default-content\">\n                <span>Middle pane<div id=\"middlepanetexts\">size: 40%</div><div id=\"middlepanetexts1\">min: 60px</div></span>\n            </div>\n            <div class=\"splitter-default-content\">\n                <span>Bottom pane<div id=\"bottompanetext\">size: 30%</div><div id=\"bottompanetext1\">min: 60px</div></span>\n            </div>\n        </div>\n    </div>\n</div>\n\n\n<style>\n    #leftpanetext, #leftpanetext1, #middlepanetext, #middlepanetext1,\n    #rightpanetext, #rightpanetext1, #toppanetext, #toppanetext1,\n    #middlepanetexts, #middlepanetexts1, #bottompanetext, #bottompanetext1 {\n        font-size: 11px;\n    }\n\n    #horizontal-pane-heading, #vertical-pane-heading {\n        font-size: 14px;\n        margin-bottom: 4px;\n    }\n    .control-section{            \n        min-height: 370px;\n        margin-bottom: 15px;\n        margin-top: 10px;    \n    }\n    .splitter-default-content {\n        text-align: center;\n        align-items: center;\n        justify-content: center;\n        display: flex;\n        height: 100%;\n    }\n    .e-rtl .splitter-default-content {\t\n        justify-content: initial;\n\t    text-align: initial;\n    }\n</style>\n\n\n\n</div></body></html>","package.json":"{\n  \"@syncfusion/ej2-base\": \"*\",\n  \"markdown-spellcheck\": \"^1.3.1\",\n  \"@syncfusion/ej2-layouts\": \"*\",\n  \"@types/crossroads\": \"0.0.28\",\n  \"@types/signals\": \"0.0.16\",\n  \"@types/moment-timezone\": \"^0.5.3\",\n  \"@types/hasher\": \"0.0.27\",\n  \"fuse.js\": \"^3.2.0\",\n  \"marked\": \"5.1.2\",\n  \"codemirror\": \"^5.37.0\",\n  \"crossroads\": \"^0.12.2\",\n  \"@types/marked\": \"5.0.1\",\n  \"@types/codemirror\": \"0.0.56\",\n  \"@types/es6-promise\": \"0.0.28\",\n  \"hasher\": \"^1.2.0\",\n  \"moment-timezone\": \"^0.5.14\"\n}","index.ts":"{{ripple}}\nimport { Splitter } from '@syncfusion/ej2-layouts';\n/**\n *  Sample for default functionalities\n */\n\n    \n    let splitObj1: Splitter = new Splitter({\n        height: '110px',\n        paneSettings: [\n            { size: '25%', min: '60px' },\n            { size: '50%', min: '60px' },\n            { size: '25%', min: '60px' }\n        ],\n        width: '100%',\n        separatorSize: 4\n    });\n    splitObj1.appendTo('#horizontal');\n    let splitObj2: Splitter = new Splitter({\n        height: '240px',\n        paneSettings: [\n            { size: '30%', min: '60px' },\n            { size: '40%', min: '60px' },\n            { size: '30%', min: '60px' }\n        ],\n        width: '100%',\n        orientation: 'Vertical',\n        separatorSize: 4\n    });\n    splitObj2.appendTo('#vertical');\n\n"}