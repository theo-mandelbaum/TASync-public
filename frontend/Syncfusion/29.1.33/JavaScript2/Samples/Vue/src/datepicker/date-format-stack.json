{"public/index.html":"<!DOCTYPE html>\r\n<html lang=\"en\">\r\n\r\n<head>\r\n    <meta charset=\"utf-8\">\r\n    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">\r\n    <meta name=\"viewport\" content=\"width=device-width,initial-scale=1.0\">\r\n    <link rel=\"icon\" href=\"<%= BASE_URL %>favicon.ico\">\r\n    <title>\r\n        <%= htmlWebpackPlugin.options.title %>\r\n    </title>\r\n    <link href=\"https://cdn.syncfusion.com/ej2/28.1.33/{{theme}}.css\" rel=\"stylesheet\">\r\n    <link href=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css\" rel=\"stylesheet\" />\r\n</head>\r\n\r\n<body>\r\n    <noscript>\r\n        <strong>We're sorry but <%= htmlWebpackPlugin.options.title %> doesn't work properly without JavaScript enabled.\r\n        Please enable it to continue.</strong>\r\n    </noscript>\r\n    <div id=\"app\"></div>\r\n    <style>\r\n            .control-section\r\n            {\r\n                margin-top: 100px;\r\n            }\r\n    </style>\r\n    <!-- built files will be auto injected -->\r\n</body>\r\n\r\n</html>\r\n","src/App.vue":"<template>\n  <div class=\"container-fluid\">\n    <div class=\"row\">\n      <div class=\"col-lg-8 control-section dateformat sb-property-border\">\n        <div class=\"content-wrapper\">\n          <div class=\"content\">\n            <div id=\"control_wrapper\" class=\"control_wrapper date-format\">\n              <ejs-datepicker :value=\"currentDate\" :placeholder=\"waterMarkText\" :format=\"selectedFormat\" :inputFormats=\"inputFormats\"></ejs-datepicker>\n            </div>\n          </div>\n        </div>\n      </div>\n      \n      <div class=\"col-lg-4\">\n        <div class=\"property-panel\">\n          <div class=\"property-panel-header\">Properties</div>\n          <div class=\"property-panel-content\">\n            <div class=\"row\">\n              <div class=\"col-xs-12 col-sm-12 col-lg-12 col-md-12\">\n                <label class=\"example-label\">Choose a display format</label>\n                <ejs-dropdownlist\n                  v-model:value=\"selectedFormat\"\n                  :dataSource=\"formatOptions\"\n                  :placeholder=\"dropdownPlaceholder\"\n                ></ejs-dropdownlist>\n              </div>\n            </div>\n            <div class=\"row mt-3\">\n              <div class=\"col-xs-12 col-sm-12 col-lg-12 col-md-12\">\n                <label class=\"example-label\">Choose input format</label>\n                <ejs-multiselect\n                  v-model:value=\"inputFormats\"\n                  :dataSource=\"inputFormatOptions\"\n                  :mode=\"multiMode\"\n                  :placeholder=\"multiselectPlaceholder\"\n                  :showSelectAll=\"showSelectAll\"\n                ></ejs-multiselect>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    \n    <div class=\"row\">\n      <div class=\"col-lg-12\">\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { DatePickerComponent } from \"@syncfusion/ej2-vue-calendars\";\nimport { DropDownListComponent, MultiSelectComponent, CheckBoxSelection } from \"@syncfusion/ej2-vue-dropdowns\";\nexport default {\n    components: {\n        'ejs-datepicker': DatePickerComponent,\n        'ejs-dropdownlist': DropDownListComponent,\n        'ejs-multiselect': MultiSelectComponent\n    },\n  data() {\n    return {\n      dateValue: null,\n      currentDate: new Date(),\n      selectedFormat: 'dd-MMM-yy',\n      datePickerPlaceholder: 'Choose a Date',\n      dropdownPlaceholder: 'Select a format',\n      multiMode: 'CheckBox',\n      showSelectAll: true,\n      multiselectPlaceholder: 'e.g. MM/dd/yyyy',\n      inputFormats: ['dd/MM/yyyy', 'yyyyMMdd'],\n      formatOptions: [ \"dd-MMM-yy\", \"yyyy-MM-dd\", \"dd-MMMM-yyyy\" ],\n      inputFormatOptions: [\n        'dd/MM/yyyy', 'ddMMMyy', 'yyyyMMdd', 'dd.MM.yy', 'MM/dd/yyyy', 'yyyy/MMM/dd', 'dd-MM-yyyy'\n      ]\n    };\n  },\n  provide: {\n    multiselect: [CheckBoxSelection]\n  }\n};\n</script>\n\n<style scoped>\n.control_wrapper.date-format {\n    max-width: 246px;\n    margin: 30px auto;\n    padding-top: 15px;\n}\n\n.control-section {\n    padding: 5%;\n}\n\n#control_wrapper #container {\n    min-height: 300px;\n}\n\n.content {\n    width: 43%;\n    margin: 0 auto;\n    min-width: 185px;\n    padding: 20px 0px;\n}\n\n.property-panel {\n    margin: 30px 10px;\n    padding: 15px;\n}\n\n.property-panel-header {\n    font-size: 14px;\n    font-weight: 600;\n    padding: 10px 0;\n}\n\n\n.example-label {\n    display: block;\n    margin-bottom: 10px;\n    font-weight: 500;\n}\n\n.mt-3 {\n    margin-top: 30px;\n}\n\n#action-description, #description {\n    padding: 15px;\n    margin-top: 10px;\n}\n</style>","dependencies":"{\"vue\":\"3.3.4\",\"@syncfusion/ej2-base\":\"*\",\"@syncfusion/ej2-vue-base\":\"*\",\"@syncfusion/ej2-calendars\":\"*\",\"@syncfusion/ej2-vue-calendars\":\"*\",\"@syncfusion/ej2-dropdowns\":\"*\",\"@syncfusion/ej2-vue-dropdowns\":\"*\",\"@syncfusion/ej2-vue-charts\":\"*\",\"@syncfusion/ej2-vue-maps\":\"*\"}","src/main.js":"import { createApp } from 'vue'\nimport App from './App.vue'\ncreateApp(App).mount('#app')"}