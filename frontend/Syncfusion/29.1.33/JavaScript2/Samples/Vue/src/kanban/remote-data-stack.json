{"public/index.html":"<!DOCTYPE html>\r\n<html lang=\"en\">\r\n\r\n<head>\r\n    <meta charset=\"utf-8\">\r\n    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">\r\n    <meta name=\"viewport\" content=\"width=device-width,initial-scale=1.0\">\r\n    <link rel=\"icon\" href=\"<%= BASE_URL %>favicon.ico\">\r\n    <title>\r\n        <%= htmlWebpackPlugin.options.title %>\r\n    </title>\r\n    <link href=\"https://cdn.syncfusion.com/ej2/28.1.33/{{theme}}.css\" rel=\"stylesheet\">\r\n    <link href=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css\" rel=\"stylesheet\" />\r\n</head>\r\n\r\n<body>\r\n    <noscript>\r\n        <strong>We're sorry but <%= htmlWebpackPlugin.options.title %> doesn't work properly without JavaScript enabled.\r\n        Please enable it to continue.</strong>\r\n    </noscript>\r\n    <div id=\"app\"></div>\r\n    <style>\r\n            .control-section\r\n            {\r\n                margin-top: 100px;\r\n            }\r\n    </style>\r\n    <!-- built files will be auto injected -->\r\n</body>\r\n\r\n</html>\r\n","src/App.vue":"<template>\n<div>\n    <div class=\"col-md-12 control-section\">\n      <div class=\"content-wrapper\">\n        <ejs-kanban id=\"kanban\" keyField=\"Status\" :dataSource=\"data\"\n        :cardSettings=\"cardSettings\" :allowDragAndDrop=false :dialogOpen='dialogOpen'>\n          <e-columns>\n            <e-column headerText=\"To Do\" keyField=\"Open\"></e-column>\n            <e-column headerText=\"In Progress\" keyField=\"InProgress\"></e-column>\n            <e-column headerText=\"Testing\" keyField=\"Testing\"></e-column>\n            <e-column headerText=\"Done\" keyField=\"Close\"></e-column>\n          </e-columns>\n        </ejs-kanban>\n      </div>\n      </div>\n\n</div>\n</template>\n<script>\nimport { extend } from \"@syncfusion/ej2-base\";\nimport { DataManager } from '@syncfusion/ej2-data';\nimport { KanbanComponent, ColumnDirective, ColumnsDirective } from \"@syncfusion/ej2-vue-kanban\";\n\nexport default {\n  components: {\n    'ejs-kanban': KanbanComponent,\n    'e-column': ColumnDirective,\n    'e-columns': ColumnsDirective\n  },\n  data: function() {\n        let SERVICE_URI =\n      'http://localhost:62728/api/Kanban'\n    return {\n         data: new DataManager({\n        url: SERVICE_URI \n      }),\n      cardSettings: {\n        contentField: \"Summary\",\n        headerField: \"Id\",\n      },\n      \n    };\n  },\n  provide: {\n    kanban: []\n  },\n  methods: {\n    dialogOpen: function (args) {\n        args.cancel = true;\n    },\n  }\n}\n</script>\n","dependencies":"{\"vue\":\"3.3.4\",\"@syncfusion/ej2-icons\":\"*\",\"markdown-spellcheck\":\"^1.3.1\",\"@syncfusion/ej2-base\":\"*\",\"@syncfusion/ej2-data\":\"*\",\"@syncfusion/ej2-vue-base\":\"*\",\"@syncfusion/ej2-kanban\":\"*\",\"@syncfusion/ej2-vue-kanban\":\"*\",\"@syncfusion/ej2-vue-charts\":\"*\",\"@syncfusion/ej2-vue-maps\":\"*\",\"@syncfusion/ej2-vue-calendars\":\"*\",\"@syncfusion/ej2-vue-grids\":\"*\",\"@syncfusion/ej2-vue-schedule\":\"*\",\"@syncfusion/ej2-vue-richtexteditor\":\"*\",\"@syncfusion/ej2-vue-inputs\":\"*\",\"@syncfusion/ej2-vue-navigations\":\"*\",\"@syncfusion/ej2-vue-documenteditor\":\"*\",\"@syncfusion/ej2-vue-splitbuttons\":\"*\",\"@syncfusion/ej2-vue-buttons\":\"*\",\"@syncfusion/ej2-vue-dropdowns\":\"*\"}","src/main.js":"import { createApp } from 'vue'\nimport App from './App.vue'\ncreateApp(App).mount('#app')"}