{"public/index.html":"<!DOCTYPE html>\r\n<html lang=\"en\">\r\n\r\n<head>\r\n    <meta charset=\"utf-8\">\r\n    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">\r\n    <meta name=\"viewport\" content=\"width=device-width,initial-scale=1.0\">\r\n    <link rel=\"icon\" href=\"<%= BASE_URL %>favicon.ico\">\r\n    <title>\r\n        <%= htmlWebpackPlugin.options.title %>\r\n    </title>\r\n    <link href=\"https://cdn.syncfusion.com/ej2/28.1.33/{{theme}}.css\" rel=\"stylesheet\">\r\n    <link href=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css\" rel=\"stylesheet\" />\r\n</head>\r\n\r\n<body>\r\n    <noscript>\r\n        <strong>We're sorry but <%= htmlWebpackPlugin.options.title %> doesn't work properly without JavaScript enabled.\r\n        Please enable it to continue.</strong>\r\n    </noscript>\r\n    <div id=\"app\"></div>\r\n    <style>\r\n            .control-section\r\n            {\r\n                margin-top: 100px;\r\n            }\r\n    </style>\r\n    <!-- built files will be auto injected -->\r\n</body>\r\n\r\n</html>\r\n","src/App.vue":"<template let-data>\n  <div class=\"card-template\">\n    <div class=\"card-template-wrap\">\n      <table class=\"card-template-wrap\">\n        <colgroup>\n          <col style=\"width: 55px\" />\n          <col />\n        </colgroup>\n        <tbody>\n          <tr>\n            <td class=\"e-image\">\n              <img :src=\"getImage(data.ImageURL)\" :alt=\"data.ImageURL\" />\n            </td>\n            <td class=\"e-title\">\n              <div class=\"e-card-stacked\">\n                <div class=\"e-card-header\">\n                  <div class=\"e-card-header-caption\">\n                    <div class=\"e-card-header-title e-tooltip-text\">\n                      {{ data.Title }}\n                    </div>\n                  </div>\n                </div>\n                <div class=\"e-card-content\" style=\"line-height: 2.75em\">\n                  <table class=\"card-template-wrap\" style=\"table-layout: auto\">\n                    <tbody>\n                      <tr>\n                        <td\n                          colspan=\"2\"\n                          v-if=\"\n                            data.Category == 'Menu' ||\n                            data.Category == 'Order' ||\n                            data.Category == 'Ready to Serve'\n                          \"\n                        >\n                          <div\n                            class=\"e-description e-tooltip-text\"\n                            v-if=\"data.Category == 'Menu'\"\n                          >\n                            {{ data.Description }}\n                          </div>\n                          <div\n                            class=\"e-description e-tooltip-text\"\n                            v-if=\"data.Category != 'Menu'\"\n                          >\n                            {{ data.OrderID }}\n                          </div>\n                        </td>\n                        <td\n                          class=\"card-content\"\n                          v-if=\"\n                            data.Category == 'Delivered'\">\n                          <table>\n                            <tr>\n                              <td class=\"e-description e-tooltip-text\">\n                                {{ data.OrderID }}\n                              </td>\n                              <td class=\"e-icons e-done\"></td>\n                            </tr>\n                            <tr>\n                              <td>\n                                <label class=\"e-date\">Deliver:</label>\n                                <span class=\"e-kanban-date\">{{ data.Date }}</span>\n                              </td>\n                            </tr>\n                          </table>\n                        </td>\n                      </tr>\n                      <tr>\n                        <td class=\"card-content\" v-if=\"data.Category == 'Menu'\">\n                          <div class=\"e-size e-tooltip-text\">\n                            {{ data.Size }}\n                          </div>\n                          <div class=\"e-price e-tooltip-text\">\n                            {{ data.Price }}\n                          </div>\n                        </td>\n                        <td class=\"card-content\" v-if=\"data.Category != 'Menu'\">\n                          <div\n                            class=\"e-preparingText e-tooltip-text\"\n                            v-if=\"data.Category == 'Order'\"\n                          >\n                            Preparing\n                          </div>\n                          <div\n                            class=\"e-readyText e-tooltip-text\"\n                            v-if=\"data.Category == 'Ready to Serve'\"\n                          >\n                            Ready to Serve\n                          </div>\n                          <div\n                            class=\"e-deliveredText e-tooltip-text\"\n                            v-if=\"\n                              data.Category == 'Delivered'\">\n                            Delivered\n                          </div>\n                          <div\n                            class=\"e-time e-tooltip-text\"\n                            v-if=\"data.Category == 'Order'\"\n                          >\n                            <div class=\"e-icons e-clock\"></div>\n                            <div class=\"e-mins\">15 mins</div>\n                          </div>\n                          <div\n                            class=\"e-time e-tooltip-text\"\n                            v-if=\"data.Category == 'Ready to Serve'\"\n                          >\n                            <div class=\"e-icons e-clock\"></div>\n                            <div class=\"e-mins\">5 mins</div>\n                          </div>\n                        </td>\n                      </tr>\n                    </tbody>\n                  </table>\n                </div>\n              </div>\n            </td>\n          </tr>\n        </tbody>\n      </table>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Vue from \"vue\";\n\nexport default {\n  data() {\n    return {\n      data: {},\n    };\n  },\n  methods: {\n    getImage: function (url) {\n      return \"source/kanban/images/\" + url;\n    },\n  },\n};\n</script>","dependencies":"{\"vue\":\"3.3.4\",\"@syncfusion/ej2-vue-charts\":\"*\",\"@syncfusion/ej2-base\":\"*\",\"@syncfusion/ej2-vue-maps\":\"*\",\"@syncfusion/ej2-vue-calendars\":\"*\",\"@syncfusion/ej2-data\":\"*\",\"@syncfusion/ej2-vue-grids\":\"*\",\"@syncfusion/ej2-vue-schedule\":\"*\",\"@syncfusion/ej2-vue-richtexteditor\":\"*\",\"@syncfusion/ej2-vue-inputs\":\"*\",\"@syncfusion/ej2-vue-navigations\":\"*\",\"@syncfusion/ej2-vue-documenteditor\":\"*\",\"@syncfusion/ej2-vue-splitbuttons\":\"*\",\"@syncfusion/ej2-vue-buttons\":\"*\",\"@syncfusion/ej2-vue-dropdowns\":\"*\"}","src/main.js":"import { createApp } from 'vue'\nimport App from './App.vue'\ncreateApp(App).mount('#app')"}