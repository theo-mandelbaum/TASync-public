{"public/index.html":"<!DOCTYPE html>\r\n<html lang=\"en\">\r\n\r\n<head>\r\n    <meta charset=\"utf-8\">\r\n    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">\r\n    <meta name=\"viewport\" content=\"width=device-width,initial-scale=1.0\">\r\n    <link rel=\"icon\" href=\"<%= BASE_URL %>favicon.ico\">\r\n    <title>\r\n        <%= htmlWebpackPlugin.options.title %>\r\n    </title>\r\n    <link href=\"https://cdn.syncfusion.com/ej2/28.1.33/{{theme}}.css\" rel=\"stylesheet\">\r\n    <link href=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css\" rel=\"stylesheet\" />\r\n</head>\r\n\r\n<body>\r\n    <noscript>\r\n        <strong>We're sorry but <%= htmlWebpackPlugin.options.title %> doesn't work properly without JavaScript enabled.\r\n        Please enable it to continue.</strong>\r\n    </noscript>\r\n    <div id=\"app\"></div>\r\n    <style>\r\n            .control-section\r\n            {\r\n                margin-top: 100px;\r\n            }\r\n    </style>\r\n    <!-- built files will be auto injected -->\r\n</body>\r\n\r\n</html>\r\n","src/App.vue":"<template>\n<div>\n<table>\n    <tbody>\n        <tr>\n            <td class=\"e-label\">ID</td>\n            <td>\n              <div class=\"e-float-input e-control-wrapper\">\n                <input id=\"Id\" name=\"Id\" type=\"text\" class=\"e-field\" v-model='data.Id' disabled />\n              </div>\n            </td>\n        </tr>\n        <tr>\n            <td class=\"e-label\">Category</td>\n            <td>\n               <ejs-dropdownlist id=\"Category\" name=\"Category\" class=\"e-field\" v-model='data.Category' :dataSource=\"dataSource1\" placeholder=\"Category\"></ejs-dropdownlist>\n            </td>\n        </tr>\n        <tr>\n            <td class=\"e-label\">Title</td>\n            <td>\n              <ejs-textbox id=\"Title\" name=\"Title\" v-model='data.Title' class=\"e-field\" placeholder=\"Title\"></ejs-textbox>\n            </td>\n        </tr>\n        <tr>\n            <td class=\"e-label\">Size</td>\n            <td>\n              <ejs-textbox type=\"Size\" name=\"Size\" id=\"Size\" v-model='data.Size' class=\"e-field\" placeholder=\"Size\"></ejs-textbox>\n            </td>\n        </tr>\n        <tr>\n            <td class=\"e-label\">Description</td>\n            <td>\n              <div class=\"e-float-input e-control-wrapper\">\n                <textarea type=\"text\" name=\"Description\" id=\"Description\" class=\"e-field\" v-model='data.Description'></textarea>\n              </div>\n            </td>\n        </tr>\n         <tr>\n            <td class=\"e-label\">Deliver</td>\n            <td>\n              <ejs-datepicker id=\"datepicker\" v-model='data.Date' format=\"MM/dd/yyyy\"></ejs-datepicker>\n            </td>\n        </tr>\n    </tbody>\n</table>\n</div>\n</template>\n<script>\nimport { DropDownListComponent } from '@syncfusion/ej2-vue-dropdowns';\nimport { TextBoxComponent } from '@syncfusion/ej2-vue-inputs';\nimport { DatePickerComponent } from '@syncfusion/ej2-vue-calendars';\nexport default {\n  components: {\n    'ejs-dropdownlist': DropDownListComponent,\n    'ejs-textbox': TextBoxComponent,\n    'ejs-datepicker': DatePickerComponent,\n  },\n  data() {\n    var categoryData = [\n      { text: 'Menu' },\n      { text: 'Order' },\n      { text: 'Ready to Serve' },\n      { text: 'Delivered' },\n    ];\n    return {\n      dataSource1: categoryData,\n      data: {},\n    };\n}\n};\n</script>","dependencies":"{\"vue\":\"3.3.4\",\"@syncfusion/ej2-base\":\"*\",\"@syncfusion/ej2-vue-base\":\"*\",\"@syncfusion/ej2-dropdowns\":\"*\",\"@syncfusion/ej2-vue-dropdowns\":\"*\",\"@syncfusion/ej2-inputs\":\"*\",\"@syncfusion/ej2-vue-inputs\":\"*\",\"@syncfusion/ej2-calendars\":\"*\",\"@syncfusion/ej2-vue-calendars\":\"*\",\"@syncfusion/ej2-vue-charts\":\"*\",\"@syncfusion/ej2-vue-maps\":\"*\",\"@syncfusion/ej2-data\":\"*\",\"@syncfusion/ej2-vue-grids\":\"*\",\"@syncfusion/ej2-vue-schedule\":\"*\",\"@syncfusion/ej2-vue-richtexteditor\":\"*\",\"@syncfusion/ej2-vue-navigations\":\"*\",\"@syncfusion/ej2-vue-documenteditor\":\"*\",\"@syncfusion/ej2-vue-splitbuttons\":\"*\",\"@syncfusion/ej2-vue-buttons\":\"*\"}","src/main.js":"import { createApp } from 'vue'\nimport App from './App.vue'\ncreateApp(App).mount('#app')"}