{"public/index.html":"<!DOCTYPE html>\r\n<html lang=\"en\">\r\n\r\n<head>\r\n    <meta charset=\"utf-8\">\r\n    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">\r\n    <meta name=\"viewport\" content=\"width=device-width,initial-scale=1.0\">\r\n    <link rel=\"icon\" href=\"<%= BASE_URL %>favicon.ico\">\r\n    <title>\r\n        <%= htmlWebpackPlugin.options.title %>\r\n    </title>\r\n    <link href=\"https://cdn.syncfusion.com/ej2/28.1.33/{{theme}}.css\" rel=\"stylesheet\">\r\n    <link href=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css\" rel=\"stylesheet\" />\r\n</head>\r\n\r\n<body>\r\n    <noscript>\r\n        <strong>We're sorry but <%= htmlWebpackPlugin.options.title %> doesn't work properly without JavaScript enabled.\r\n        Please enable it to continue.</strong>\r\n    </noscript>\r\n    <div id=\"app\"></div>\r\n    <style>\r\n            .control-section\r\n            {\r\n                margin-top: 100px;\r\n            }\r\n    </style>\r\n    <!-- built files will be auto injected -->\r\n</body>\r\n\r\n</html>\r\n","src/App.vue":"<template>\n  <main><div>\n    <div class=\"control-section\">    \n        <ejs-lineargauge :load='load' ref=\"lineargauge\" :background='background' :annotations='annotations' style='display:block' align='center' id='boxContainer' :container='container' >                  \n            <e-axes>\n                <e-axis minimum=-20 maximum=50 :line='line'  :majorTicks='majorTicks' :minorTicks='minorTicks' :labelStyle='labelStyle'>\n                    <e-pointers>\n                        <e-pointer value=35 :height='height' :width='width' :markerType='markerType' :color='color1' :offset='offset' :placement='placement'></e-pointer>\n                    </e-pointers>\n                </e-axis>\n                <e-axis minimum=0 maximum=120 :line='line2'  :minorTicks='minorTicks2'  :majorTicks='majorTicks2' :labelStyle='labelStyle' :opposedPosition='opposedPosition'>\n                    <e-pointers>\n                        <e-pointer value=94 :type='type2' :color='color2' :height='height2' :width='width2'></e-pointer>\n                    </e-pointers>\n                </e-axis>\n            </e-axes>\n        </ejs-lineargauge>     \n    </div>\n<br>\n</div>\n<section id=\"action-description\" aria-label=\"Description of Linear Gauge sample\">\n    <p>\n        This sample shows a thermometer that displays temperature in both degrees and fahrenheit.\n    </p>\n</section>\n<section id=\"description\" aria-label=\"Description of the Linear Gauge features demonstrated in this sample\">\n    <p>\n        In this example, you can see how to render and configure a linear gauge to look like a thermometer. More\n        information about containers can be found in this <a target=\"_blank\"\n            href=\"https://ej2.syncfusion.com/vue/documentation/linear-gauge/linear-gauge-appearance#customizing-the-linear-gauge-container\">documentation\n            section</a>.\n    </p>\n</section>\n</main>\n</template>\n<script>\nimport { LinearGaugeComponent, AxisDirective, AxesDirective, PointersDirective, PointerDirective, Annotations } from \"@syncfusion/ej2-vue-lineargauge\";\nimport { DropDownListComponent } from '@syncfusion/ej2-vue-dropdowns';\n\nexport default {\n    components: {\n        'ejs-lineargauge': LinearGaugeComponent,\n        'e-axes': AxesDirective,\n        'e-axis': AxisDirective,\n        'e-pointers': PointersDirective,\n        'e-pointer': PointerDirective,\n        'ejs-dropdownlist': DropDownListComponent,\n    },\n    data: function () {\n        return {\n            container: {\n                width: 13,\n                roundedCornerRadius: 5,\n                type: 'Thermometer',\n                border: { width: 1 }\n            },\n            background:'transparent',\n            line: {\n                width: 0\n            },\n            labelStyle:{\n                font: { fontFamily: 'inherit' }\n            },\n            majorTicks: {\n                interval: 10, height: 7\n            },\n            minorTicks: {\n                height: 0, interval: 5\n            },\n            height: 12,\n            width: 12, \n            color1: '#0074E3',\n            placement: 'Center',\n            offset: 3,\n            markerType: 'Triangle',\n            type2: 'Bar',\n            color2:'#0074E3',\n            height2:13,\n            line2: {\n                width: 0\n            },\n            majorTicks2: {\n                interval: 20, height: 7\n            },\n            minorTicks2: {\n                height: 0, interval: 5\n            },\n            opposedPosition: true,\n            width2: 13,\n            annotations: [\n            {\n                content: '<div style=\"font-size:13px;margin-left: -30px;margin-top: -30px;\"> \\u00b0C </div>',\n                axisIndex: 0,\n                axisValue: 50,\n                x: 0,\n                y: 0, zIndex: '1'\n            },\n            {\n                content: '<div style=\"font-size:13px;margin-left: 15px;margin-top: -30px;\"> \\u00b0F </div>',\n                axisIndex: 1,\n                axisValue: 120,\n                x: 12,\n                y: 0, zIndex: '1'\n            }\n        ]\n        }\n    },\n    provide: {\n    lineargauge: [Annotations]\n    },\n    methods: {    \n    load: function(args) {\n      /* custom code start */\n      let selectedTheme = location.hash.split(\"/\")[1];\n      selectedTheme = selectedTheme ? selectedTheme : \"Material\";\n      args.gauge.theme =\n        (selectedTheme.charAt(0).toUpperCase() +\n            selectedTheme.slice(1)).replace(/-dark/i, 'Dark').replace(/-high/i, 'High').replace(/contrast/i, 'Contrast').replace(/5.3/i, '5');\n      /* custom code end */\n    }\n}\n};\n</script> ","dependencies":"{\"vue\":\"3.3.4\",\"@syncfusion/ej2-base\":\"*\",\"@syncfusion/ej2-vue-base\":\"*\",\"@syncfusion/ej2-lineargauge\":\"*\",\"@syncfusion/ej2-vue-lineargauge\":\"*\",\"@syncfusion/ej2-dropdowns\":\"*\",\"@syncfusion/ej2-vue-dropdowns\":\"*\",\"@syncfusion/ej2-vue-charts\":\"*\",\"@syncfusion/ej2-vue-maps\":\"*\",\"@syncfusion/ej2-vue-calendars\":\"*\",\"@syncfusion/ej2-data\":\"*\",\"@syncfusion/ej2-vue-grids\":\"*\",\"@syncfusion/ej2-vue-schedule\":\"*\",\"@syncfusion/ej2-vue-richtexteditor\":\"*\",\"@syncfusion/ej2-vue-inputs\":\"*\",\"@syncfusion/ej2-vue-navigations\":\"*\",\"@syncfusion/ej2-vue-documenteditor\":\"*\",\"@syncfusion/ej2-vue-splitbuttons\":\"*\",\"@syncfusion/ej2-vue-buttons\":\"*\"}","src/main.js":"import { createApp } from 'vue'\nimport App from './App.vue'\ncreateApp(App).mount('#app')"}