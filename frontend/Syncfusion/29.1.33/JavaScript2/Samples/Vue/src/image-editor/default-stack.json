{"public/index.html":"<!DOCTYPE html>\r\n<html lang=\"en\">\r\n\r\n<head>\r\n    <meta charset=\"utf-8\">\r\n    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">\r\n    <meta name=\"viewport\" content=\"width=device-width,initial-scale=1.0\">\r\n    <link rel=\"icon\" href=\"<%= BASE_URL %>favicon.ico\">\r\n    <title>\r\n        <%= htmlWebpackPlugin.options.title %>\r\n    </title>\r\n    <link href=\"https://cdn.syncfusion.com/ej2/28.1.33/{{theme}}.css\" rel=\"stylesheet\">\r\n    <link href=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css\" rel=\"stylesheet\" />\r\n</head>\r\n\r\n<body>\r\n    <noscript>\r\n        <strong>We're sorry but <%= htmlWebpackPlugin.options.title %> doesn't work properly without JavaScript enabled.\r\n        Please enable it to continue.</strong>\r\n    </noscript>\r\n    <div id=\"app\"></div>\r\n    <style>\r\n            .control-section\r\n            {\r\n                margin-top: 100px;\r\n            }\r\n    </style>\r\n    <!-- built files will be auto injected -->\r\n</body>\r\n\r\n</html>\r\n","src/App.vue":"<template>\r\n    <div class=\"control-section\">\r\n        <div class=\"col-lg-12 e-img-editor-sample\">\r\n            <ejs-imageeditor :theme=\"themeValue\" id=\"image-editor\" ref=\"imageEditorObj\" :created = \"created\">\r\n                \r\n            </ejs-imageeditor>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<!-- custom code start -->\r\n<style scoped>\r\n    \r\n    .e-img-editor-sample {\r\n        height: 80vh;\r\n        width: 100%;\r\n    }\r\n\r\n    @media only screen and (max-width: 700px) {\r\n        .e-img-editor-sample {\r\n            height: 75vh;\r\n            width: 100%;\r\n        }\r\n    }\r\n\r\n\t.control-wrapper {\r\n\t\theight: 100%;\r\n\t}\r\n\r\n    .e-image-editor {\r\n        margin: 0 auto;\r\n    }\r\n</style>\r\n<!-- custom code end -->\r\n\r\n<script>\r\nimport { ImageEditorComponent } from \"@syncfusion/ej2-vue-image-editor\";\r\nimport { Browser, getComponent, isNullOrUndefined } from \"@syncfusion/ej2-base\";\r\n\r\nexport default {\r\n  components: {\r\n    'ejs-imageeditor': ImageEditorComponent\r\n  },\r\n  data: function() {\r\n    return {\r\n        theme: 'Bootstrap5'\r\n    };\r\n  },\r\n  methods: {\r\n    created: function() {\r\n        if (Browser.isDevice) {\r\n            this.$refs.imageEditorObj.open('https://ej2.syncfusion.com/vue/demos/src/image-editor/images/flower.png');\r\n        } else {\r\n            this.$refs.imageEditorObj.open('https://ej2.syncfusion.com/vue/demos/src/image-editor/images/default.png');\r\n        }\r\n        if (this.themeValue && window.location.href.split('#')[1]) {\r\n            this.themeValue = window.location.href.split('#')[1].split('/')[1];\r\n        } \r\n    },\r\n    onScroll: function() {\r\n        if (document.getElementById('imageeditor_sliderWrapper')) {\r\n            var slider = getComponent(document.getElementById('imageeditor_sliderWrapper'), 'slider');\r\n            slider.refreshTooltip(slider.tooltipTarget);\r\n        }\r\n    }\r\n  },\r\n  mounted: function() {\r\n    if (!isNullOrUndefined(document.getElementById(\"right-pane\"))) {\r\n        document.getElementById(\"right-pane\").addEventListener(\"scroll\", this.onScroll.bind(this));\r\n    }\r\n  },\r\n  computed: {\r\n    themeValue: {\r\n        get: function () {\r\n            return this.theme;\r\n        },\r\n        set: function (theme) {\r\n            this.theme = theme\r\n        }\r\n    }\r\n  }\r\n};\r\n</script>\r\n","dependencies":"{\"vue\":\"3.3.4\",\"@syncfusion/ej2-base\":\"*\",\"@syncfusion/ej2-vue-base\":\"*\",\"@syncfusion/ej2-image-editor\":\"*\",\"@syncfusion/ej2-vue-image-editor\":\"*\",\"@syncfusion/ej2-icons\":\"*\",\"markdown-spellcheck\":\"^1.3.1\",\"@syncfusion/ej2-vue-charts\":\"*\",\"@syncfusion/ej2-vue-maps\":\"*\",\"@syncfusion/ej2-vue-calendars\":\"*\",\"@syncfusion/ej2-data\":\"*\",\"@syncfusion/ej2-vue-grids\":\"*\",\"@syncfusion/ej2-vue-schedule\":\"*\",\"@syncfusion/ej2-vue-richtexteditor\":\"*\",\"@syncfusion/ej2-vue-inputs\":\"*\",\"@syncfusion/ej2-vue-navigations\":\"*\",\"@syncfusion/ej2-vue-documenteditor\":\"*\",\"@syncfusion/ej2-vue-splitbuttons\":\"*\",\"@syncfusion/ej2-vue-buttons\":\"*\",\"@syncfusion/ej2-vue-dropdowns\":\"*\"}","src/main.js":"import { createApp } from 'vue'\nimport App from './App.vue'\ncreateApp(App).mount('#app')"}