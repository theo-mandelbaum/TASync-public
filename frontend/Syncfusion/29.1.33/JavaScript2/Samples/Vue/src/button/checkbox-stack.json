{"public/index.html":"<!DOCTYPE html>\r\n<html lang=\"en\">\r\n\r\n<head>\r\n    <meta charset=\"utf-8\">\r\n    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">\r\n    <meta name=\"viewport\" content=\"width=device-width,initial-scale=1.0\">\r\n    <link rel=\"icon\" href=\"<%= BASE_URL %>favicon.ico\">\r\n    <title>\r\n        <%= htmlWebpackPlugin.options.title %>\r\n    </title>\r\n    <link href=\"https://cdn.syncfusion.com/ej2/28.1.33/{{theme}}.css\" rel=\"stylesheet\">\r\n    <link href=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css\" rel=\"stylesheet\" />\r\n</head>\r\n\r\n<body>\r\n    <noscript>\r\n        <strong>We're sorry but <%= htmlWebpackPlugin.options.title %> doesn't work properly without JavaScript enabled.\r\n        Please enable it to continue.</strong>\r\n    </noscript>\r\n    <div id=\"app\"></div>\r\n    <style>\r\n            .control-section\r\n            {\r\n                margin-top: 100px;\r\n            }\r\n    </style>\r\n    <!-- built files will be auto injected -->\r\n</body>\r\n\r\n</html>\r\n","src/App.vue":"<template>\n  <div class=\"control-section\">\n    <div class=\"checkbox-control\">\n        <div class=\"row\">\n           <ejs-checkbox :label=\"chkLabel\" :checked=\"true\" :change=\"onChange\"></ejs-checkbox>\n        </div>\n        <div class=\"row\">\n            <ejs-checkbox label='Checked  Disabled' :disabled=\"true\" :checked=\"true\"></ejs-checkbox>\n        </div>\n        <div class=\"row\">\n            <ejs-checkbox label='Indeterminate, Disabled' :indeterminate=\"true\" :disabled=\"true\"></ejs-checkbox>\n        </div>\n    </div>\n</div>\n</template>\n<!-- custom code start -->\n<style scoped>\n/* CheckBox Styles */\n.checkbox-control {\n  margin: 12% 0 12% 40%;\n}\n\n@media only screen and (max-width: 700px) {\n  .checkbox-control {\n    margin: 20% 0 0 32%;\n  }\n\n  .control-section {\n    min-height: 200px;\n  }\n}\n\n@media only screen and (max-width: 500px) {\n  .control-section {\n    min-height: 200px;\n  }\n\n  .checkbox-control {\n    margin-top: 27%;\n    margin-left: 20%;\n    margin-bottom: 27%;\n  }\n}\n\n.control-section .row {\n  margin: 20px 0;\n}\n</style>\n<!-- custom code end -->\n<script>\nimport { CheckBoxComponent } from \"@syncfusion/ej2-vue-buttons\";\n\nexport default {\n  components: { 'ejs-checkbox': CheckBoxComponent },\n  data: function () {\n    return {\n      label: \"CheckBox: true\"\n    };\n  },\n  computed: {\n    chkLabel: {\n      get: function () {\n        return this.label;\n      },\n      set: function (label) {\n        this.label = label;\n      }\n    }\n  },\n  methods: {\n    onChange: function(args) {\n     this.label = \"CheckBox: \" + args.checked;\n    }\n  }\n}\n</script>","dependencies":"{\"vue\":\"3.3.4\",\"@syncfusion/ej2-base\":\"*\",\"@syncfusion/ej2-vue-base\":\"*\",\"@syncfusion/ej2-buttons\":\"*\",\"@syncfusion/ej2-vue-buttons\":\"*\"}","src/main.js":"import { createApp } from 'vue'\nimport App from './App.vue'\ncreateApp(App).mount('#app')"}