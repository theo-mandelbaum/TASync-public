{"public/index.html":"<!DOCTYPE html>\r\n<html lang=\"en\">\r\n\r\n<head>\r\n    <meta charset=\"utf-8\">\r\n    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">\r\n    <meta name=\"viewport\" content=\"width=device-width,initial-scale=1.0\">\r\n    <link rel=\"icon\" href=\"<%= BASE_URL %>favicon.ico\">\r\n    <title>\r\n        <%= htmlWebpackPlugin.options.title %>\r\n    </title>\r\n    <link href=\"https://cdn.syncfusion.com/ej2/28.1.33/{{theme}}.css\" rel=\"stylesheet\">\r\n    <link href=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css\" rel=\"stylesheet\" />\r\n</head>\r\n\r\n<body>\r\n    <noscript>\r\n        <strong>We're sorry but <%= htmlWebpackPlugin.options.title %> doesn't work properly without JavaScript enabled.\r\n        Please enable it to continue.</strong>\r\n    </noscript>\r\n    <div id=\"app\"></div>\r\n    <style>\r\n            .control-section\r\n            {\r\n                margin-top: 100px;\r\n            }\r\n    </style>\r\n    <!-- built files will be auto injected -->\r\n</body>\r\n\r\n</html>\r\n","src/App.vue":"<template>\n<div class=\"control-section\">\n    <div class=\"sb-drop-down-section\">\n        <div id=\"sb-drop-down-control\">\n            <div class=\"row\">\n                <div class=\"col-xs-12 col-sm-12 col-lg-6 col-md-6\">\n                    <ejs-dropdownbutton iconCss=\"e-drpdwnbtn-icons e-ddb-profile\" :items=\"items\"></ejs-dropdownbutton>\n                </div>\n                <div class=\"col-xs-12 col-sm-12 col-lg-6 col-md-6\">\n                    <ejs-dropdownbutton :items=\"items\">Profile</ejs-dropdownbutton>\n                </div>\n                <div class=\"col-xs-12 col-sm-12 col-lg-6 col-md-6\">\n                    <ejs-dropdownbutton :items=\"items\" iconCss=\"e-drpdwnbtn-icons e-ddb-profile\">Profile</ejs-dropdownbutton>\n                </div>\n                <div class=\"col-xs-12 col-sm-12 col-lg-6 col-md-6\">\n                    <ejs-dropdownbutton :items=\"items\" cssClass=\"e-caret-hide\">Profile</ejs-dropdownbutton>\n                </div>\n            </div>\n        </div>\n    </div>\n</div>\n</template>\n\n<style>\n/* custom code start */\n#sb-drop-down-control {\n  width: 100%;\n  margin: 11% 0;\n  text-align: center;\n}\n\n.sb-drop-down-section {\n  width: 80%;\n  margin: auto;\n}\n\n#sb-drop-down-control .col-xs-12 {\n  margin: 15px 0;\n}\n\n@media only screen and (max-width: 500px) {\n  #sb-drop-down-control .col-lg-6 {\n    margin-bottom: 30px;\n  }\n}\n\n@media only screen and (min-width: 1200px) {\n  #sb-drop-down-control .col-lg-6 {\n    width: 25%;\n  }\n}\n/* custom code end */\n@font-face {\n  font-family: \"e-drpbtn-icons\";\n  src: url(data:application/x-font-ttf;charset=utf-8;base64,) format('truetype');\n  font-weight: normal;\n  font-style: normal;\n}\n\n.e-drpdwnbtn-icons {\n  font-family: \"e-drpbtn-icons\" !important;\n  speak: none;\n  font-size: 55px;\n  font-style: normal;\n  font-weight: normal;\n  font-variant: normal;\n  text-transform: none;\n  line-height: 1;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n}\n\n.e-ddb-logout::before {\n  content: \"\\\\e702\";\n}\n\n.e-ddb-dashboard::before {\n  content: \"\\\\e704\";\n}\n\n.e-ddb-profile::before {\n  content: \"\\\\e703\";\n}\n\n.e-ddb-notifications::before {\n  content: \"\\\\e700\";\n}\n\n.e-ddb-settings::before {\n  content: \"\\\\e701\";\n}\n\n.material .e-ddb-logout::before,\n.bootstrap .e-ddb-logout::before {\n  content: \"\\\\e706\";\n}\n\n.material .e-ddb-dashboard::before,\n.bootstrap .e-ddb-dashboard::before {\n  content: \"\\\\e708\";\n}\n\n.material .e-ddb-profile::before,\n.bootstrap .e-ddb-profile::before,\n.bootstrap4 .e-ddb-profile::before {\n  content: \"\\\\e811\";\n}\n\n.material .e-ddb-notifications::before,\n.bootstrap .e-ddb-notifications::before {\n  content: \"\\\\e707\";\n}\n\n.material .e-ddb-settings::before,\n.bootstrap .e-ddb-settings::before {\n  content: \"\\\\e705\";\n}\n\n.bootstrap4 .e-ddb-logout::before {\n  content: \"\\\\e816\";\n}\n\n.bootstrap4 .e-ddb-dashboard::before {\n    content: \"\\\\e80f\";\n}\n\n.bootstrap4 .e-ddb-notifications::before {\n  content: \"\\\\e810\";\n}\n\n.bootstrap4 .e-ddb-settings::before {\n  content: \"\\\\e815\";\n}\n</style>\n\n<script>\nimport { DropDownButtonComponent } from \"@syncfusion/ej2-vue-splitbuttons\";\n\nexport default {\n  components: { 'ejs-dropdownbutton': DropDownButtonComponent },\n  data: function() {\n    return {\n      items: [\n        {\n          text: \"Dashboard\",\n          iconCss: \"e-drpdwnbtn-icons e-ddb-dashboard\"\n        },\n        {\n          text: \"Notifications\",\n          iconCss: \"e-drpdwnbtn-icons e-ddb-notifications\"\n        },\n        {\n          text: \"User Settings\",\n          iconCss: \"e-drpdwnbtn-icons e-ddb-settings\"\n        },\n        {\n          text: \"Log Out\",\n          iconCss: \"e-drpdwnbtn-icons e-ddb-logout\"\n        }\n      ]\n    };\n  }\n}\n</script>","dependencies":"{\"vue\":\"3.3.4\",\"@syncfusion/ej2-base\":\"*\",\"@syncfusion/ej2-vue-base\":\"*\",\"@syncfusion/ej2-splitbuttons\":\"*\",\"@syncfusion/ej2-vue-splitbuttons\":\"*\"}","src/main.js":"import { createApp } from 'vue'\nimport App from './App.vue'\ncreateApp(App).mount('#app')"}