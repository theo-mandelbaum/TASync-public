{"public/index.html":"<!DOCTYPE html>\r\n<html lang=\"en\">\r\n\r\n<head>\r\n    <meta charset=\"utf-8\">\r\n    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">\r\n    <meta name=\"viewport\" content=\"width=device-width,initial-scale=1.0\">\r\n    <link rel=\"icon\" href=\"<%= BASE_URL %>favicon.ico\">\r\n    <title>\r\n        <%= htmlWebpackPlugin.options.title %>\r\n    </title>\r\n    <link href=\"https://cdn.syncfusion.com/ej2/28.1.33/{{theme}}.css\" rel=\"stylesheet\">\r\n    <link href=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css\" rel=\"stylesheet\" />\r\n</head>\r\n\r\n<body>\r\n    <noscript>\r\n        <strong>We're sorry but <%= htmlWebpackPlugin.options.title %> doesn't work properly without JavaScript enabled.\r\n        Please enable it to continue.</strong>\r\n    </noscript>\r\n    <div id=\"app\"></div>\r\n    <style>\r\n            .control-section\r\n            {\r\n                margin-top: 100px;\r\n            }\r\n    </style>\r\n    <!-- built files will be auto injected -->\r\n</body>\r\n\r\n</html>\r\n","src/App.vue":"<template>\n<div>\n  <div class=\"control-section\">\n      <div class=\"content-wrapper breadcrumb-control-wrapper\">\n          <div class=\"row material2\">\n              <div class=\"col-xs-12 col-sm-12 col-lg-12 col-md-12\">\n                  <h5 style=\"display: inline-block\">File Manager like Breadcrumb</h5>\n                  <ejs-button id='reset' class=\"reset-btn e-small\" v-on:click=\"btnClick\">Reset State</ejs-button>\n              </div>\n          </div>\n          <div class=\"row material2\">\n              <div class=\"col-xs-12 col-sm-12 col-lg-12 col-md-12\">\n                  <ejs-breadcrumb ref=\"breadcrumb\" id=\"breadcrumb\" :items=\"breadcrumbItems\" cssClass=\"e-addressbar-breadcrumb\" :itemTemplate=\"itemTemplate\" :separatorTemplate=\"separatorTemplate\">\n                  </ejs-breadcrumb>\n              </div>\n          </div>\n      </div>\n</div>\n\n</div>\n</template>\n\n<!-- custom code start -->\n<style>\n    .breadcrumb-control-wrapper {\n  width: 90% !important;\n  margin: 0 auto;\n  min-width: 85px;\n}\n\n.breadcrumb-control-wrapper div.row {\n  padding: 15px 0px;\n}\n\n@media only screen and (max-width: 480px) {\n  .breadcrumb-control-wrapper {\n      width: 92%;\n  }\n\n  .col-xs-12,\n  .col-xs-4,\n  .col-xs-12 {\n      padding: 10px 5px;\n      width: 100%;\n  }\n\n  .breadcrumb-control-wrapper div.row {\n      padding: 0px;\n  }\n}\n\n.e-addressbar-breadcrumb {\n  margin-left: 0 !important;\n}\n\n.bootstrap .e-addressbar-breadcrumb ol,\n.bootstrap4 .e-addressbar-breadcrumb ol,\n.bootstrap-dark .e-addressbar-breadcrumb ol {\n  margin-left: -16px;\n}\n\n.bootstrap5-dark .e-addressbar-breadcrumb .e-menu-wrapper ul .e-menu-item.e-menu-caret-icon,\n.bootstrap5 .e-addressbar-breadcrumb .e-menu-wrapper ul .e-menu-item.e-menu-caret-icon {\n  margin-top: -1px;\n}\n\n\n.e-bigger.bootstrap5-dark .e-addressbar-breadcrumb .e-menu-wrapper ul .e-menu-item.e-menu-caret-icon,\n.e-bigger.bootstrap5 .e-addressbar-breadcrumb .e-menu-wrapper ul .e-menu-item.e-menu-caret-icon {\n  margin-top: -2px;\n  padding-right: 26px !important;\n}\n\n.e-bigger.fabric-dark .e-addressbar-breadcrumb .e-menu-wrapper ul .e-menu-item.e-menu-caret-icon {\n  padding-right: 25px !important;\n}\n.e-bigger.fabric .e-addressbar-breadcrumb .e-menu-wrapper ul .e-menu-item.e-menu-caret-icon,\n.e-bigger.highcontrast .e-addressbar-breadcrumb .e-menu-wrapper ul .e-menu-item.e-menu-caret-icon {\n  padding-right: 22px !important;\n}\n.fluent .e-addressbar-breadcrumb .e-menu-wrapper ul .e-menu-item .e-caret,\n.fluent-dark .e-addressbar-breadcrumb .e-menu-wrapper ul .e-menu-item .e-caret\n{\n    right: 2px;\n}\n.e-bigger .fluent .e-addressbar-breadcrumb .e-menu-wrapper ul .e-menu-item .e-caret,\n.e-bigger .fluent-dark .e-addressbar-breadcrumb .e-menu-wrapper ul .e-menu-item .e-caret\n{\n    right: 1px;\n}\n.material3 .e-addressbar-breadcrumb .e-menu-wrapper ul .e-menu-item .e-caret,\n.material3-dark .e-addressbar-breadcrumb .e-menu-wrapper ul .e-menu-item .e-caret,\n.e-bigger .material3 .e-addressbar-breadcrumb .e-menu-wrapper ul .e-menu-item .e-caret,\n.e-bigger .material3-dark .e-addressbar-breadcrumb .e-menu-wrapper ul .e-menu-item .e-caret,\n.tailwind3 .e-addressbar-breadcrumb .e-menu-wrapper ul .e-menu-item .e-caret,\n.tailwind3-dark .e-addressbar-breadcrumb .e-menu-wrapper ul .e-menu-item .e-caret,\n.e-bigger .tailwind3 .e-addressbar-breadcrumb .e-menu-wrapper ul .e-menu-item .e-caret,\n.e-bigger .tailwind3-dark .e-addressbar-breadcrumb .e-menu-wrapper ul .e-menu-item .e-caret\n{\n    right: 2px;\n}\n</style>\n<!-- custom code end -->\n<style>\n    .e-addressbar-breadcrumb .e-menu-wrapper ul .e-menu-item.e-menu-caret-icon {\n  padding-right: 20px !important;\n  padding-left: 0px !important;\n  overflow: hidden;\n}\n\n.tailwind .e-addressbar-breadcrumb .e-menu-wrapper ul .e-menu-item.e-menu-caret-icon,\n.tailwind-dark .e-addressbar-breadcrumb .e-menu-wrapper ul .e-menu-item.e-menu-caret-icon,\n.e-bigger.bootstrap-dark .e-addressbar-breadcrumb .e-menu-wrapper ul .e-menu-item.e-menu-caret-icon {\n  padding-right: 25px !important;\n}\n\n.bootstrap5 .e-addressbar-breadcrumb .e-menu-wrapper ul .e-menu-item.e-menu-caret-icon,\n.bootstrap5-dark .e-addressbar-breadcrumb .e-menu-wrapper ul .e-menu-item.e-menu-caret-icon {\n  padding-right: 23px !important;\n  margin-top: -2px;\n}\n\n.e-addressbar-breadcrumb .e-menu-wrapper ul .e-menu-item .e-caret {\n  right: 5px;\n}\n\n.e-addressbar-breadcrumb .e-breadcrumb-item,\n.e-addressbar-breadcrumb .e-breadcrumb-separator {\n  padding: 0px !important;\n}\n\n.fabric .e-addressbar-breadcrumb .e-menu {\n  padding: 0 8px !important;\n}\n\n.fabric .e-bigger .e-addressbar-breadcrumb .e-menu {\n  padding: 0 12px !important;\n}\n\n.fabric .e-addressbar-breadcrumb .e-breadcrumb-item .e-menu,\n.highcontrast .e-addressbar-breadcrumb .e-breadcrumb-item .e-menu,\n.fabric-dark .e-addressbar-breadcrumb .e-breadcrumb-item .e-menu {\n  padding: 0 !important;\n}\n\n.e-addressbar-breadcrumb .e-menu-wrapper {\n  border: none;\n}\n\n.e-addressbar-breadcrumb ol {\n  background-color: transparent !important;\n}\n\n.e-addressbar-breadcrumb .e-menu-wrapper ul .e-menu-item .e-menu-icon {\n  margin-right: 0;\n}\n\n.fabric-dark .e-addressbar-breadcrumb .e-menu-wrapper ul .e-menu-item .e-menu-icon,\n.fabric .e-addressbar-breadcrumb .e-menu-wrapper ul .e-menu-item .e-menu-icon,\n.highcontrast .e-addressbar-breadcrumb .e-menu-wrapper ul .e-menu-item .e-menu-icon  {\n  padding-right: 22px;\n  padding-left: 6px;\n}\n\n.bootstrap .e-addressbar-breadcrumb .e-menu-wrapper ul .e-menu-item .e-menu-icon {\n  padding-right: 16px;\n}\n\n.e-addressbar-breadcrumb .e-caret {\n  transform: rotate(-90deg);\n}\n\n.e-addressbar-breadcrumb .e-open .e-caret {\n  transform: none;\n}\n\n@font-face {\n  font-family: 'e-breadcrumb-addressbar-icons';\n  src:\n      url(data:application/x-font-ttf;charset=utf-8;base64,) format('truetype');\n  font-weight: normal;\n  font-style: normal;\n}\n\n.e-addressbar-breadcrumb .e-bicons {\n  font-family: 'e-breadcrumb-addressbar-icons' !important;\n}\n\n.e-addressbar-breadcrumb .e-folder::before {\n  content: '\\\\e705';\n}\n\n.e-addressbar-breadcrumb .e-onedrive::before {\n  content: '\\\\e713';\n}\n\n.e-addressbar-breadcrumb .e-pc::before {\n  content: '\\\\e711';\n}\n\n.e-addressbar-breadcrumb .e-desktop::before {\n  content: '\\\\e700';\n}\n\n.e-addressbar-breadcrumb .e-documents::before {\n  content: '\\\\e70d';\n}\n\n.e-addressbar-breadcrumb .e-downloads::before {\n  content: '\\\\e709';\n}\n\n.e-addressbar-breadcrumb .e-picture::before {\n  content: '\\\\e718';\n}\n\n.e-addressbar-breadcrumb .e-network::before {\n  content: '\\\\e71c';\n}\n\n.e-addressbar-breadcrumb .e-recyclebin::before {\n  content: '\\\\e71f';\n}\n\n.reset-btn {\n  float: right;\n  margin: 5px 2px 5px 0;\n}\n</style>\n\n<script>\nimport { createApp } from 'vue';\nimport {  BreadcrumbComponent, MenuComponent } from \"@syncfusion/ej2-vue-navigations\";\nimport { ButtonComponent } from \"@syncfusion/ej2-vue-buttons\";\nimport { getComponent } from \"@syncfusion/ej2-base\";\n\nvar app = createApp({});\n\nexport default {\n    components: {\n        'ejs-breadcrumb': BreadcrumbComponent,\n        'ejs-button': ButtonComponent\n    },\n    data: function() {\n        return {\n            itemTemplate:() => {\n                return {\n                    template : app.component('itemTemplate', {\n                        components: {\n                            'ejs-menu': MenuComponent,\n                        },\n                        template:\n                            `<div v-if=\"data.text !== 'LastItem'\" style=\"display: flex;\">\n                            <ejs-menu :items=\"[{ text: data.text, iconCss: data.iconCss }]\" :select=\"selectHandler\"></ejs-menu>\n                            </div>\n                            `,\n                            data: function() {\n                              return {data: {}}\n                            },\n                            mixins: [this],\n                            methods: {\n                                selectHandler: function (args) {\n                                    var breadcrumb = getComponent(document.getElementById('breadcrumb'), 'breadcrumb');\n                                    var breadcrumbItems = breadcrumb.items || this.$options.breadcrumbItems;\n                                    for (var i = 0; i < breadcrumbItems.length; i++) {\n                                        if (breadcrumbItems[i].text === args.item.text) {\n                                            breadcrumbItems = breadcrumbItems.slice(0, i + 1);\n                                            const itemsData = this.$options.getItems(args.item.text, true)[0] || {};\n                                            const newItemType = itemsData.items ? itemsData.items.type : null;\n                                            const newIconCss = newItemType ? 'e-bicons e-' + newItemType : breadcrumbItems[0].iconCss;\n\n                                            breadcrumbItems[0].iconCss = newIconCss;\n                                            breadcrumb.items = breadcrumbItems;\n                                            break;\n                                        }\n                                    }\n                                    breadcrumb.items.push({ text: 'LastItem' });\n                                    breadcrumb.activeItem = 'LastItem';\n                                }\n                            }\n                    })\n                }\n            },\n            separatorTemplate:() => {\n                return {\n                    template : app.component('separatorTemplate', {\n                        components: {\n                            'ejs-menu': MenuComponent,\n                        },\n                        template:\n                            `<div v-if=\"getItems(data.previousItem.text)[0].items\" style=\"display: flex;\">\n                            <ejs-menu :showItemOnClick=\"true\" :items=\"getItems(data.previousItem.text)\" :select=\"subMenuSelectHandler\" :beforeOpen=\"beforeOpen\" :onClose=\"onClose\"></ejs-menu>\n                            </div>\n                            `,\n                            data: function() {\n                              return {data: {}}\n                            },\n                            mixins: [this],\n                            methods: {\n                                getItems: function(text, needParent) {\n                                    return this.$options.getItems(text, needParent);\n                                },\n                                subMenuSelectHandler: function (args) {\n                                    if (!args.element.parentElement.classList.contains('e-menu') && (args.item).parentObj.items[0] && (args.item).parentObj.items[0].items) {\n                                        var breadcrumb = getComponent(document.getElementById('breadcrumb'), 'breadcrumb');\n                                        var breadcrumbItems = breadcrumb.items || this.$options.breadcrumbItems;\n                                        var subItems = (args.item).parentObj.items;\n                                        var idx;\n\n                                        for (let i = 0; i < subItems.length; i++) {\n                                            for (let j = 0; j < breadcrumbItems.length; j++) {\n                                                if (subItems[i].text === breadcrumbItems[j].text) {\n                                                    idx = j;\n                                                    break;\n                                                }\n                                            }\n                                        }\n                                        if (idx !== undefined) {\n                                            breadcrumbItems = breadcrumbItems.slice(0, idx);\n                                        }\n\n                                        const newItemIcon = (args.item).type || breadcrumbItems[0].iconCss.split(' ')[1];\n                                        const newIconCss = newItemIcon ? `e-bicons e-${newItemIcon}` : breadcrumbItems[0].iconCss;\n\n                                        breadcrumbItems[0].iconCss = newIconCss;\n\n                                        if (breadcrumbItems[breadcrumbItems.length - 1].text === 'LastItem') {\n                                            breadcrumbItems.pop();\n                                        }\n                                        breadcrumbItems.push({ text: args.item.text });\n                                        breadcrumbItems.push({ text: 'LastItem' });\n                                        breadcrumb.items = breadcrumbItems;\n                                    }\n                                },\n                                beforeOpen: function() {\n                                    this.$el.classList.add('e-open');\n                                },\n                                onClose: function () {\n                                    this.$el.classList.remove('e-open');\n                                }\n                            }\n                    })\n                }\n            },\n            breadcrumbItems: [\n            {\n                iconCss: 'e-bicons e-picture'\n            },\n            {\n                text: 'This PC'\n            },\n            {\n                text: 'Local Disk (C:)'\n            },\n            {\n                text: 'Users'\n            },\n            {\n                text: 'Admin'\n            },\n            {\n                text: 'Pictures'\n            }\n            ],\n            items: [\n                {\n                    text: 'OneDrive', type: 'onedrive',\n                        items: [\n                                    { text: 'Documents', type: 'folder' },\n                                    { text: 'Email attachments', type: 'folder' },\n                                    { text: 'Music', type: 'folder' },\n                                    { text: 'Pictures', type: 'folder' }\n                                ]\n                                },\n                                            {\n                                            text: 'This PC', type: 'desktop',\n                                            items: [\n                                                { text: 'Desktop', type: 'desktop' },\n                                                { text: 'Documents', type: 'documents', items: [\n                                                    { text: 'IISExpress', type: 'folder', items: [\n                                                    { text: 'config', type: 'folder' }\n                                            ] },\n                                                { text: 'Visual Studio 2019', type: 'folder', items: [\n                                                { text: 'Code Snippets', type: 'folder' },\n                                                { text: 'Templates', type: 'folder' },\n                                                { text: 'Visualizers', type: 'folder' }\n                                            ] }\n                                            ] },\n                                            { text: 'Downloads', type: 'downloads' },\n                                            {\n                                            text: 'Local Disk (C:)', type: 'folder', items: [\n                                                {\n                                                    text: 'Microsoft', type: 'folder'\n                                                },\n                                                    { text: 'Program Files', type: 'folder', items: [ \n                                                { text: 'Git', type: 'folder', items: [\n                                                { text: 'bin', type: 'folder' },\n                                                { text: 'cmd', type: 'folder' },\n                                                { text: 'dev', type: 'folder' }\n                                            ] },\n                                            { text: 'Google', type: 'folder', items:[\n                                                { text: 'Chrome', type: 'folder' }\n                                            ] },\n                                            { text: 'Internet Explorer', type: 'folder', items:[\n                                                { text: 'en-US', type: 'folder' }\n                                            ]  }\n                                        ] },\n                            { text: 'Program Files (x86)', type: 'folder', items:[\n                            { text: 'Microsoft', type: 'folder', items: [\n                                { text: 'Edge', type: 'folder' }\n                            ] },\n                            { text: 'MSBuild', type: 'folder' },\n                            { text: 'Windows Defender', type: 'folder' }\n                        ] },\n                        {\n                            text: 'Users', type: 'folder', items: [\n                                {\n                                    text: 'Admin', type: 'folder', items: [\n                                        { text: 'Desktop', type: 'desktop' },\n                                        { text: 'Documents', type: 'documents' },\n                                        { text: 'Downloads', type: 'downloads' },\n                                        { text: 'Pictures', type: 'picture' }\n                                    ]\n                                },\n                                { text: 'Public', type: 'folder' }\n                            ]\n                        },\n                        { text: 'Windows', type: 'folder', items: [\n                            { text: 'Boot', type: 'folder' },\n                            { text: 'System32', type: 'folder', items: [\n                                { text: 'Configuration', type: 'folder' },\n                                { text: 'LogFiles', type: 'folder' }\n                            ] }\n                        ] }\n                    ]\n                },\n                { text: 'Local Disk (D:)', type: 'folder' }\n            ]\n        },\n        { text: 'Libraries', type: 'folder' },\n        { text: 'Network', type: 'network' },\n        { text: 'Recycle Bin', type: 'recyclebin' }\n        ]\n        }\n    },\n    methods: {\n        getItems: function(text, needParent) {\n            var breadcrumbItems = this.$refs.breadcrumb.items || this.breadcrumbItems;\n            var mItems = [].slice.call(this.items);\n            var isBreaked;\n            if (!text) {\n                mItems = this.getSubMenuItems(mItems);\n            }\n            else {\n                for (let i = 1; i < breadcrumbItems.length; i++) {\n                    for (let j = 0; j < mItems.length; j++) {\n                        if (mItems[j].text === breadcrumbItems[i].text) {\n                            if (mItems[j].text === text) {\n                                if (needParent) {\n                                    mItems = mItems[j];\n                                } else {\n                                    mItems = this.getSubMenuItems(mItems[j].items);\n                                }\n                                isBreaked = true;\n                            } else {\n                                mItems = mItems[j].items;\n                                j = 0;\n                                if(mItems == null) {\n                                    isBreaked = true;\n                                }\n                            }\n                            break;\n                        }\n                    }\n                    if (isBreaked) {\n                        break;\n                    }\n                }\n            }\n            return [{ items: mItems }];\n        },\n        getSubMenuItems: function(mItems) {\n            let subItems;\n            if (mItems) {\n                subItems = [];\n                for (let i = 0; i < mItems.length; i++) {\n                    subItems.push({ text: mItems[i].text, type: (mItems[i]).type });\n                }\n            }\n            return subItems;\n        },\n    btnClick: function() {\n        var breadcrumb = getComponent(document.getElementById('breadcrumb'), 'breadcrumb');\n        breadcrumb.items = [\n            {\n                iconCss: 'e-bicons e-picture'\n            },\n            {\n                text: 'This PC'\n            },\n            {\n                text: 'Local Disk (C:)'\n            },\n            {\n                text: 'Users'\n            },\n            {\n                text: 'Admin'\n            },\n            {\n                text: 'Pictures'\n            }\n        ]\n    }\n}\n};\n</script>","dependencies":"{\"vue\":\"3.3.4\",\"@syncfusion/ej2-base\":\"*\",\"@syncfusion/ej2-vue-base\":\"*\",\"@syncfusion/ej2-navigations\":\"*\",\"@syncfusion/ej2-vue-navigations\":\"*\",\"@syncfusion/ej2-buttons\":\"*\",\"@syncfusion/ej2-vue-buttons\":\"*\",\"@syncfusion/ej2-icons\":\"*\",\"markdown-spellcheck\":\"^1.3.1\"}","src/main.js":"import { createApp } from 'vue'\nimport App from './App.vue'\ncreateApp(App).mount('#app')"}