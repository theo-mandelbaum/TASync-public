{"public/index.html":"<!DOCTYPE html>\r\n<html lang=\"en\">\r\n\r\n<head>\r\n    <meta charset=\"utf-8\">\r\n    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">\r\n    <meta name=\"viewport\" content=\"width=device-width,initial-scale=1.0\">\r\n    <link rel=\"icon\" href=\"<%= BASE_URL %>favicon.ico\">\r\n    <title>\r\n        <%= htmlWebpackPlugin.options.title %>\r\n    </title>\r\n    <link href=\"https://cdn.syncfusion.com/ej2/28.1.33/{{theme}}.css\" rel=\"stylesheet\">\r\n    <link href=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css\" rel=\"stylesheet\" />\r\n</head>\r\n\r\n<body>\r\n    <noscript>\r\n        <strong>We're sorry but <%= htmlWebpackPlugin.options.title %> doesn't work properly without JavaScript enabled.\r\n        Please enable it to continue.</strong>\r\n    </noscript>\r\n    <div id=\"app\"></div>\r\n    <style>\r\n            .control-section\r\n            {\r\n                margin-top: 100px;\r\n            }\r\n    </style>\r\n    <!-- built files will be auto injected -->\r\n</body>\r\n\r\n</html>\r\n","src/App.vue":"<template>\n  <div>\n     <div id=\"target\" class=\"col-lg-12 control-section resizable\">\n        <ejs-button id='dlgbtn' v-if=\"ShowBtn\" v-on:click=\"buttonClick\">Open Dialog</ejs-button>\n\n        <ejs-dialog header='Resize Me!!!' ref=\"dialogObj\" :resizeHandles='resizeHandles' allowDragging='true' :animationSettings='animationSettings' content='This is a dialog with resizable support.' enableResize='true' showCloseIcon='true' :target='target' :width='width' :open=\"dialogOpen\"\n            :close=\"dialogClose\">\n        </ejs-dialog>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { DialogComponent } from '@syncfusion/ej2-vue-popups';\nimport { ButtonComponent } from '@syncfusion/ej2-vue-buttons';\n\nexport default {\n    components: {\n      'ejs-dialog': DialogComponent,\n      'ejs-button': ButtonComponent\n    },\n    data: function() {\n        return {\n            target: '#target',\n            animationSettings:  { effect: 'None' },\n            resizeHandles: ['All'],\n            width:  '300px',\n            ShowBtn: false            \n        }\n    },\n    methods: {\n        buttonClick: function(){\n            this.$refs.dialogObj.show();\n        },\n        dialogClose: function() {\n            this.ShowBtn = true;\n        },\n        dialogOpen: function() {\n            this.ShowBtn = false;\n        }\n    }\n}\n</script>\n\n<style scoped>\n    /* custom code start */\n    .control-section{\n        min-height: 350px;    \n    }\n    /* custom code end */\n    .material .resizable .e-dialog, .bootstrap .resizable .e-dialog, \n    .bootstrap4 .resizable .e-dialog {\n        min-height: 125px;\n    }\n    .fabric .resizable .e-dialog, .highcontrast .resizable .e-dialog {\n        min-height: 135px;\n    }\n</style>\n","dependencies":"{\"vue\":\"3.3.4\",\"@syncfusion/ej2-base\":\"*\",\"@syncfusion/ej2-vue-base\":\"*\",\"@syncfusion/ej2-popups\":\"*\",\"@syncfusion/ej2-vue-popups\":\"*\",\"@syncfusion/ej2-buttons\":\"*\",\"@syncfusion/ej2-vue-buttons\":\"*\",\"@syncfusion/ej2-vue-charts\":\"*\",\"@syncfusion/ej2-vue-maps\":\"*\",\"@syncfusion/ej2-vue-calendars\":\"*\",\"@syncfusion/ej2-data\":\"*\",\"@syncfusion/ej2-vue-grids\":\"*\",\"@syncfusion/ej2-vue-schedule\":\"*\",\"@syncfusion/ej2-vue-richtexteditor\":\"*\",\"@syncfusion/ej2-vue-inputs\":\"*\",\"@syncfusion/ej2-vue-navigations\":\"*\"}","src/main.js":"import { createApp } from 'vue'\nimport App from './App.vue'\ncreateApp(App).mount('#app')"}