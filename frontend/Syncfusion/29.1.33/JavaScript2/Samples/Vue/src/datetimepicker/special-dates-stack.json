{"public/index.html":"<!DOCTYPE html>\r\n<html lang=\"en\">\r\n\r\n<head>\r\n    <meta charset=\"utf-8\">\r\n    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">\r\n    <meta name=\"viewport\" content=\"width=device-width,initial-scale=1.0\">\r\n    <link rel=\"icon\" href=\"<%= BASE_URL %>favicon.ico\">\r\n    <title>\r\n        <%= htmlWebpackPlugin.options.title %>\r\n    </title>\r\n    <link href=\"https://cdn.syncfusion.com/ej2/28.1.33/{{theme}}.css\" rel=\"stylesheet\">\r\n    <link href=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css\" rel=\"stylesheet\" />\r\n</head>\r\n\r\n<body>\r\n    <noscript>\r\n        <strong>We're sorry but <%= htmlWebpackPlugin.options.title %> doesn't work properly without JavaScript enabled.\r\n        Please enable it to continue.</strong>\r\n    </noscript>\r\n    <div id=\"app\"></div>\r\n    <style>\r\n            .control-section\r\n            {\r\n                margin-top: 100px;\r\n            }\r\n    </style>\r\n    <!-- built files will be auto injected -->\r\n</body>\r\n\r\n</html>\r\n","src/App.vue":"<template>\n<div class=\"col-lg-12 control-section\">\n    <div id=\"wrapper\">\n         <ejs-datetimepicker id=\"datetimepicker\" :renderDayCell=\"load\" :cssClass=\"cssClass\" :value=\"dateValue\"></ejs-datetimepicker>\n    </div>\n</div>\n</template>\n\n<script>\n\nimport { DateTimePickerComponent } from \"@syncfusion/ej2-vue-calendars\";\nimport { addClass } from \"@syncfusion/ej2-base\";\n\nexport default {\n  data: function() {\n    return {\n      dateValue: new Date(\"1/13/2017\"),\n      cssClass: \"datetimepicker-special-dates\"\n    };\n  },\n  components: { 'ejs-datetimepicker': DateTimePickerComponent },\n  methods: {\n    specialDate: function(args, name) {\n      let span = document.createElement(\"span\");\n      span.setAttribute(\"class\", \"e-special-icons highlight\");\n      args.element.firstElementChild.setAttribute(\"title\", name + \"!\");\n      addClass([args.element], [\"e-day\", \"special\", name.toLowerCase()]);\n      args.element.setAttribute(\"data-val\", name + \"!\");\n      args.element.setAttribute(\"title\", name + \"!\");\n      args.element.appendChild(span);\n    },\n    load: function(args) {\n      /*Date need to be customized*/\n      if (args.date.getDate() === 10) {\n        this.specialDate(args, \"Birthday\");\n      }\n      if (args.date.getDate() === 15) {\n        this.specialDate(args, \"Farewell\");\n      }\n      if (args.date.getDate() === 20) {\n        this.specialDate(args, \"Vacation\");\n      }\n    }\n  }\n};\n</script>\n\n\n\n<style scoped>\n#wrapper {\n  max-width: 246px;\n  margin: 30px auto;\n  padding-top: 15px;\n}\n\n.datepicker-special-dates .e-input-group-icon.e-date-icon::before, \n*.e-control-wrapper.datepicker-special-dates .e-input-group-icon.e-date-icon::before, \n.datepicker-special-dates.e-datetime-wrapper .e-time-icon.e-special-icons::before {\n  transform: translate(0,1px);\n}\n\n.e-bigger .datepicker-special-dates .e-input-group-icon.e-date-icon::before, \n.e-bigger *.e-control-wrapper.datepicker-special-dates .e-input-group-icon.e-date-icon::before,\n.e-bigger .datepicker-special-dates.e-datetime-wrapper .e-time-icon.e-special-icons::before {\n  transform: translate(0,2px);\n}\n\n.datepicker-special-dates.e-input-group .e-clear-icon, \n.datepicker-special-dates.e-input-group.e-control-wrapper .e-clear-icon {\n  padding-top: 4px;\n}\n\n.e-bigger .datepicker-special-dates.e-input-group .e-clear-icon, \n.e-bigger .datepicker-special-dates.e-input-group.e-control-wrapper .e-clear-icon {\n  padding-top: 5px;\n}\n\n.datetimepicker-special-dates .e-special-icons {\n  font-family: 'e-special-icons' !important;\n}\n\n@font-face {\n    font-family: 'e-special-icons';\n    src: url(data:application/x-font-ttf;charset=utf-8;base64,) format('truetype');\n    font-weight: normal;\n    font-style: normal;\n}\n\n</style>\n<style>\n  .datetimepicker-special-dates span.e-special-icons.highlight {\n  margin-top: -13px;\n  display: block;\n  margin-left: 4px;\n  font-weight: bold;\n  font-size: 14px;\n}\n\n.datetimepicker-special-dates td span.e-special-icons.highlight {\n  color: #0501fc;\n}\n\n.highcontrast .datetimepicker-special-dates td.e-special-icons.highlight,\n.highcontrast .datetimepicker-special-dates td.e-special-icons.highlight:before,\n.material-dark .datetimepicker-special-dates td.e-special-icons.highlight,\n.material-dark .datetimepicker-special-dates td.e-special-icons.highlight:before,\n.material3-dark .datetimepicker-special-dates td.e-special-icons.highlight,\n.material3-dark .datetimepicker-special-dates td.e-special-icons.highlight:before,\n.fabric-dark .datetimepicker-special-dates td.e-special-icons.highlight,\n.fabric-dark .datetimepicker-special-dates td.e-special-icons.highlight:before,\n.bootstrap-dark .datetimepicker-special-dates td.e-special-icons.highlight,\n.bootstrap-dark .datetimepicker-special-dates td.e-special-icons.highlight:before,\n.bootstrap5-dark .datetimepicker-special-dates td.e-special-icons.highlight,\n.bootstrap5-dark .datetimepicker-special-dates td.e-special-icons.highlight:before,\n.bootstrap5.3-dark .datetimepicker-special-dates td.e-special-icons.highlight,\n.bootstrap5.3-dark .datetimepicker-special-dates td.e-special-icons.highlight:before,\n.tailwind-dark .datetimepicker-special-dates td.e-special-icons.highlight,\n.tailwind3-dark .datetimepicker-special-dates td.e-special-icons.highlight,\n.tailwind-dark .datetimepicker-special-dates td.e-special-icons.highlight:before,\n.tailwind3-dark .datetimepicker-special-dates td.e-special-icons.highlight:before {\n    color: #ff7500;\n}\n\n.datetimepicker-special-dates .e-other-month span.e-special-icons.highlight:before {\n  content: \"\";\n}\n\n.datetimepicker-special-dates span.e-special-icons.highlight:before {\n  content:\"\\\\e190\";\n  vertical-align: middle;\n  margin-right: 3px;\n  font-size: 4px;\n  position: relative;\n  top: 0px;\n  font-weight: normal;\n}\n\n.e-bigger .datetimepicker-special-dates.e-calendar span.e-special-icons.highlight:before {\n  top: -3px;\n}\n\nbody.highcontrast .e-bigger .datetimepicker-special-dates.e-calendar span.e-special-icons.highlight:before {\n  top: -2px;\n}\n\nbody.highcontrast .datetimepicker-special-dates td.birthday span.e-special-icons.highlight:before {\n  color: #ff6161;\n}\n\nbody.highcontrast .datetimepicker-special-dates td.farewell span.e-special-icons.highlight:before {\n  color: #2bc700;\n}\n\nbody.highcontrast .datetimepicker-special-dates td.vacation span.e-special-icons.highlight:before {\n  color: #ff7d1a;\n}\n</style>","dependencies":"{\"vue\":\"3.3.4\",\"@syncfusion/ej2-base\":\"*\",\"@syncfusion/ej2-vue-base\":\"*\",\"@syncfusion/ej2-calendars\":\"*\",\"@syncfusion/ej2-vue-calendars\":\"*\",\"@syncfusion/ej2-icons\":\"*\",\"markdown-spellcheck\":\"^1.3.1\",\"@syncfusion/ej2-vue-charts\":\"*\",\"@syncfusion/ej2-vue-maps\":\"*\"}","src/main.js":"import { createApp } from 'vue'\nimport App from './App.vue'\ncreateApp(App).mount('#app')"}