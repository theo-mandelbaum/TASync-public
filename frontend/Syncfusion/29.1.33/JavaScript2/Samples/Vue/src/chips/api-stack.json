{"src/data.json":"{\n    \"ddlData\": [{\n        \"id\": \"1\",\n        \"text\": \"Default\"\n      },\n      {\n        \"id\": \"2\",\n        \"text\": \"Primary\"\n      },\n      {\n        \"id\": \"3\",\n        \"text\": \"Success\"\n      },\n      {\n        \"id\": \"4\",\n        \"text\": \"Danger\"\n      },\n      {\n        \"id\": \"5\",\n        \"text\": \"Warning\"\n      },\n      {\n        \"id\": \"6\",\n        \"text\": \"Info\"\n      }\n    ],\n    \"avatarData\": [{\n        \"id\": \"1\",\n        \"text\": \"None\"\n      },\n      {\n        \"id\": \"2\",\n        \"text\": \"Icon\"\n      },\n      {\n        \"id\": \"3\",\n        \"text\": \"Image\"\n      },\n      {\n        \"id\": \"4\",\n        \"text\": \"Letter\"\n      }\n    ]\n  }\n","public/index.html":"<!DOCTYPE html>\r\n<html lang=\"en\">\r\n\r\n<head>\r\n    <meta charset=\"utf-8\">\r\n    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">\r\n    <meta name=\"viewport\" content=\"width=device-width,initial-scale=1.0\">\r\n    <link rel=\"icon\" href=\"<%= BASE_URL %>favicon.ico\">\r\n    <title>\r\n        <%= htmlWebpackPlugin.options.title %>\r\n    </title>\r\n    <link href=\"https://cdn.syncfusion.com/ej2/28.1.33/{{theme}}.css\" rel=\"stylesheet\">\r\n    <link href=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css\" rel=\"stylesheet\" />\r\n</head>\r\n\r\n<body>\r\n    <noscript>\r\n        <strong>We're sorry but <%= htmlWebpackPlugin.options.title %> doesn't work properly without JavaScript enabled.\r\n        Please enable it to continue.</strong>\r\n    </noscript>\r\n    <div id=\"app\"></div>\r\n    <style>\r\n            .control-section\r\n            {\r\n                margin-top: 100px;\r\n            }\r\n    </style>\r\n    <!-- built files will be auto injected -->\r\n</body>\r\n\r\n</html>\r\n","src/App.vue":"<template>\n<div class=\"chip-api\">\n    <div class=\"col-lg-8 control-section\">\n        <div id=\"chip-api-wrapper\">\n            <!-- initialize chip component -->\n            <ejs-chiplist id=\"chip\" ref=\"chip\" text=\"Janet Leverling\"></ejs-chiplist>\n        </div>\n    </div>\n    <div class=\"col-lg-4 property-section\" id=\"chips-api-property\">\n        <table id=\"property\" title=\"Properties\">\n            <tbody>\n            <tr>\n                <td style=\"width: 50%\">\n                <div class=\"userselect\">Color</div>\n                </td>\n                <td style=\"width: 50%;padding-right: 10px\">\n                <div>\n                    <!-- initialize dropdownlist component -->\n                    <ejs-dropdownlist id=\"chip-color\" :dataSource=\"ddlData\" placeholder=\"Select a color\" :change=\"colorChange\">\n\n                    </ejs-dropdownlist>\n                </div>\n                </td>\n            </tr>\n            <tr>\n                <td style=\"width: 50%\">\n                <div class=\"userselect\">Leading icon</div>\n                </td>\n                <td style=\"width: 50%;padding-right: 10px\">\n                <div style=\"padding-left: 0;padding-top: 0\">\n                    <!-- initialize checkbox component -->\n                    <ejs-checkbox id=\"chip-leadingicon\" :change=\"iconHandler\"></ejs-checkbox>\n                </div>\n                </td>\n            </tr>\n            <tr>\n                <td style=\"width: 50%\">\n                <div class=\"userselect\">Avatar</div>\n                </td>\n                <td style=\"width: 50%;padding-right: 10px\">\n                <div>\n                    <!-- initialize dropdownlist component -->\n                    <ejs-dropdownlist id=\"chip-avatar\" :dataSource=\"avatarData\" placeholder=\"Select an avatar\" :change=\"avatarHandler\">\n\n                    </ejs-dropdownlist>\n                </div>\n                </td>\n            </tr>\n            <tr>\n                <td style=\"width: 50%\">\n                <div class=\"userselect\">Trailing icon</div>\n                </td>\n                <td style=\"width: 50%;padding-right: 10px\">\n                <div style=\"padding-left: 0;padding-top: 0\">\n                    <!-- initialize checkbox component -->\n                    <ejs-checkbox id=\"chip-trailingicon\" :change=\"deleteIconHandler\"></ejs-checkbox>\n                </div>\n                </td>\n            </tr>\n            <tr>\n                <td style=\"width: 50%\">\n                <div class=\"userselect\">Outline</div>\n                </td>\n                <td style=\"width: 50%;padding-right: 10px\">\n                <div style=\"padding-left: 0;padding-top: 0\">\n                    <!-- initialize checkbox component -->\n                    <ejs-checkbox id=\"chip-outline\" :change=\"variantHandler\"></ejs-checkbox>\n                </div>\n                </td>\n            </tr>\n            </tbody>\n        </table>\n    </div>\n\n</div>\n</template>\n\n<script>\nimport { ChipListComponent, CheckBoxComponent } from \"@syncfusion/ej2-vue-buttons\";\nimport { DropDownListComponent } from \"@syncfusion/ej2-vue-dropdowns\";\nimport data from \"./data.json\";\n\nexport default {\n  components: { \n    'ejs-chiplist': ChipListComponent, \n    'ejs-dropdownlist': DropDownListComponent, \n    'ejs-checkbox': CheckBoxComponent \n    },\n  data: function() {\n    return {\n      ddlData: data.ddlData,\n      avatarData: data.avatarData,\n      outlineCss: \"\",\n      colorCss: \"\"\n    };\n  },\n  methods: {\n    // checkbox change handler for chip leading icon\n    iconHandler: function(e) {\n      this.$refs.chip.ej2Instances.leadingIconCss = e.checked ? \"janet\" : \"\";\n    },\n    // drop-down list change handler for chip color\n    colorChange: function(e) {\n      this.$refs.chip.ej2Instances.cssClass = `e-${e.value.toLowerCase()} ${this.outlineCss.trim()}`;\n      this.colorCss = `e-${e.value.toLowerCase()}`;\n    },\n    // checkbox change handler for chip outline\n    variantHandler: function(e) {\n      this.outlineCss = e.checked ? \"e-outline\" : \"\";\n      this.$refs.chip.ej2Instances.cssClass = `${this.colorCss} ${this.outlineCss}`;\n    },\n    // drop-down list change handler for chip avatar\n    avatarHandler: function(e) {\n      this.$refs.chip.ej2Instances.avatarIconCss =\n        e.value.toLowerCase() === \"icon\"\n          ? \"e-icon\"\n          : e.value.toLowerCase() === \"image\"\n            ? \"janet\"\n            : \"\";\n      this.$refs.chip.ej2Instances.avatarText =\n        e.value.toLowerCase() === \"letter\" ? \"JL\" : \"\";\n    },\n    // checkbox change handler for chip trailing icon\n    deleteIconHandler: function(e) {\n      this.$refs.chip.ej2Instances.trailingIconCss = e.checked ? \"e-dlt-btn\" : \"\";\n    }\n  }\n}\n</script>\n\n<style>\n.chip-api #chip-api-wrapper .janet {\n  background-image: url(\"https://ej2.syncfusion.com/vue/demos/src/chips/images/janet.png\");\n}\n\n.chip-api #chip-api-wrapper {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  margin-top: 100px;\n}\n\n/* chips samples icons */\n\n.chip-api #chip .e-chip-avatar.e-icon {\n  font-family: \"e-user-icon\";\n}\n\n.chip-api #chip .e-chip-avatar.e-icon:before {\n  content: \"\\\\e700\";\n}\n\n@font-face {\n  font-family: \"e-user-icon\";\n  src: url(data:application/x-font-ttf;charset=utf-8;base64,)\n    format(\"truetype\");\n  font-weight: normal;\n  font-style: normal;\n}\n</style>","dependencies":"{\"vue\":\"3.3.4\",\"@syncfusion/ej2-base\":\"*\",\"@syncfusion/ej2-vue-base\":\"*\",\"@syncfusion/ej2-buttons\":\"*\",\"@syncfusion/ej2-vue-buttons\":\"*\",\"@syncfusion/ej2-dropdowns\":\"*\",\"@syncfusion/ej2-vue-dropdowns\":\"*\",\"@syncfusion/ej2-vue-charts\":\"*\"}","src/main.js":"import { createApp } from 'vue'\nimport App from './App.vue'\ncreateApp(App).mount('#app')"}