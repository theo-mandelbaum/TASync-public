{"public/index.html":"<!DOCTYPE html>\r\n<html lang=\"en\">\r\n\r\n<head>\r\n    <meta charset=\"utf-8\">\r\n    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">\r\n    <meta name=\"viewport\" content=\"width=device-width,initial-scale=1.0\">\r\n    <link rel=\"icon\" href=\"<%= BASE_URL %>favicon.ico\">\r\n    <title>\r\n        <%= htmlWebpackPlugin.options.title %>\r\n    </title>\r\n    <link href=\"https://cdn.syncfusion.com/ej2/28.1.33/{{theme}}.css\" rel=\"stylesheet\">\r\n    <link href=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css\" rel=\"stylesheet\" />\r\n    <link href=\"https://ej2.syncfusion.com/vue/demos/lib/codemirror.css\" rel=\"stylesheet\" />\n    <script src=\"https://cdnjs.cloudflare.com/ajax/libs/codemirror/5.3.0/codemirror.js\" type=\"text/javascript\"></script>\n    <script src=\"https://cdnjs.cloudflare.com/ajax/libs/codemirror/5.3.0/mode/css/css.js\" type=\"text/javascript\"></script>\n    <script src=\"https://cdnjs.cloudflare.com/ajax/libs/codemirror/5.3.0/mode/xml/xml.js\" type=\"text/javascript\"></script>\n    <script src=\"https://cdnjs.cloudflare.com/ajax/libs/codemirror/5.3.0/mode/htmlmixed/htmlmixed.js\" type=\"text/javascript\"></script>\n</head>\r\n\r\n<body>\r\n    <noscript>\r\n        <strong>We're sorry but <%= htmlWebpackPlugin.options.title %> doesn't work properly without JavaScript enabled.\r\n        Please enable it to continue.</strong>\r\n    </noscript>\r\n    <div id=\"app\"></div>\r\n    <style>\r\n            .control-section\r\n            {\r\n                margin-top: 100px;\r\n            }\r\n    </style>\r\n    <!-- built files will be auto injected -->\r\n</body>\r\n\r\n</html>\r\n","src/App.vue":"<template>\n    <div class=\"schedule-vue-sample\">\n        <div class=\"col-md-9 control-section\">\n            <div class=\"content-wrapper\">\n                <div class=\"schedule-demo-heading\">\n                    Cheapest one way fares from Barcelona to Los Angeles\n                </div>\n                <ejs-schedule id=\"Schedule\" width=\"100%\" height=\"650px\" :cssClass=\"cssClass\" :readonly=\"readonly\" :selectedDate=\"selectedDate\"\n                    :eventSettings=\"eventSettings\" :dataBinding=\"dataBinding\" :dataBound=\"dataBound\" :actionBegin=\"actionBegin\"\n                    :popupOpen=\"popupOpen\">\n                    <template v-slot:tooltipTemplate=\"{data}\">\n                        <div class=\"event-tooltip\">\n                            <div class='airline-header'>\n                                <div :class=\"['airline-logo ' + `${getAirwaysImage(data.AirlineId)}`]\"></div>\n                                <div class='airline-name'>{{ getAirwaysName(data.AirlineId) }}</div>\n                            </div>\n                            <div class='airline-details text-size'>\n                                <div class='airline-title'>Fare Details:</div>\n                                <div class='airline-fare'>${{ data.Fare }} per person</div>\n                            </div>\n                            <div class='airline-flex-row text-size'>\n                                <div class='airline-flex-col airline-title border-right'>Arrival</div>\n                                <div class='airline-flex-col airline-title text-right'>Depature</div>\n                            </div>\n                            <div class='airline-flex-row text-size'>\n                                <div class='airline-flex-col border-right'>{{ getFormattedTime(data.StartTime) }}</div>\n                                <div class='airline-flex-col margin-right text-right'>{{ getFormattedTime(data.EndTime) }}\n                                </div>\n                            </div>\n                        </div>\n                    </template>\n                    <e-resources>\n                        <e-resource field=\"AirlineId\" title=\"Airline\" :dataSource=\"resourceDataSource\" :allowMultiple=\"allowMultiple\" name=\"Airlines\"\n                            textField=\"text\" idField=\"id\">\n                        </e-resource>\n                    </e-resources>\n                    <e-views>\n                        <e-view option=\"Month\"></e-view>\n                    </e-views>\n                </ejs-schedule>\n            </div>\n        </div>\n        <div class=\"col-lg-3 property-section\">\n            <div id=\"property\" class=\"property-panel-table\" title=\"Properties\">\n                <table id=\"property\" title=\"Properties\" style=\"width: 100%\">\n                    <tbody>\n                        <tr style=\"height: 50px\">\n                            <td>\n                                <div>\n                                    <ejs-checkbox label=\"Airways 1\" cssClass=\"e-resource e-airways-1\" :checked=\"true\" :change=\"onChange\"></ejs-checkbox>\n                                </div>\n                            </td>\n                        </tr>\n                        <tr style=\"height: 50px\">\n                            <td>\n                                <div>\n                                    <ejs-checkbox label=\"Airways 2\" cssClass=\"e-resource e-airways-2\" :checked=\"true\" :change=\"onChange\"></ejs-checkbox>\n                                </div>\n                            </td>\n                        </tr>\n                        <tr style=\"height: 50px\">\n                            <td>\n                                <div>\n                                    <ejs-checkbox label=\"Airways 3\" cssClass=\"e-resource e-airways-3\" :checked=\"true\" :change=\"onChange\"></ejs-checkbox>\n                                </div>\n                            </td>\n                        </tr>\n                    </tbody>\n                </table>\n            </div>\n        </div>\n\n\n    </div>\n</template>\n<style>\n    .schedule-resources.e-tooltip-wrap.e-schedule-event-tooltip .airli