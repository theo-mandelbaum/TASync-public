{"public/index.html":"<!DOCTYPE html>\r\n<html lang=\"en\">\r\n\r\n<head>\r\n    <meta charset=\"utf-8\">\r\n    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">\r\n    <meta name=\"viewport\" content=\"width=device-width,initial-scale=1.0\">\r\n    <link rel=\"icon\" href=\"<%= BASE_URL %>favicon.ico\">\r\n    <title>\r\n        <%= htmlWebpackPlugin.options.title %>\r\n    </title>\r\n    <link href=\"https://cdn.syncfusion.com/ej2/28.1.33/{{theme}}.css\" rel=\"stylesheet\">\r\n    <link href=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css\" rel=\"stylesheet\" />\r\n    <link href=\"https://ej2.syncfusion.com/vue/demos/lib/codemirror.css\" rel=\"stylesheet\" />\n    <script src=\"https://cdnjs.cloudflare.com/ajax/libs/codemirror/5.3.0/codemirror.js\" type=\"text/javascript\"></script>\n    <script src=\"https://cdnjs.cloudflare.com/ajax/libs/codemirror/5.3.0/mode/css/css.js\" type=\"text/javascript\"></script>\n    <script src=\"https://cdnjs.cloudflare.com/ajax/libs/codemirror/5.3.0/mode/xml/xml.js\" type=\"text/javascript\"></script>\n    <script src=\"https://cdnjs.cloudflare.com/ajax/libs/codemirror/5.3.0/mode/htmlmixed/htmlmixed.js\" type=\"text/javascript\"></script>\n</head>\r\n\r\n<body>\r\n    <noscript>\r\n        <strong>We're sorry but <%= htmlWebpackPlugin.options.title %> doesn't work properly without JavaScript enabled.\r\n        Please enable it to continue.</strong>\r\n    </noscript>\r\n    <div id=\"app\"></div>\r\n    <style>\r\n            .control-section\r\n            {\r\n                margin-top: 100px;\r\n            }\r\n    </style>\r\n    <!-- built files will be auto injected -->\r\n</body>\r\n\r\n</html>\r\n","src/App.vue":"<template>\n    <div id=\"textarea-api\"  class=\"col-lg-8 control-section api-textarea\">\n    <div class=\"content_wrapper\">\n        <div class=\"api-row\">\n            <ejs-textarea id=\"api\" placeholder='Enter your comments' floatLabelType='Auto' :enabled='enabled' :readonly='readOnly' :showClearButton='showClearButton' :maxLength='maxLength' :value='value' :rows='rows' :cols='cols'></ejs-textarea>\n        </div>\n    </div>\n    </div>\n\n    <div class=\"col-lg-4 property-section\">\n        <table id=\"property\" title=\"Properties\" class=\"api-property\">\n\n            <tr>\n                <td class=\"left-side\"> Rows </td>\n                <td><ejs-numerictextbox id=\"rows\" format=\"##\" value=\"2\" min=\"1\" max=\"10\" :change=\"rowHandler\"></ejs-numerictextbox></td>\n            </tr>\n            <tr>\n                <td class=\"left-side\"> Columns </td>\n                <td><ejs-numerictextbox id=\"cols\" format=\"##\" value=\"20\" min=\"5\" max=\"40\" :change=\"columnHandler\"></ejs-numerictextbox></td>\n            </tr>\n            <tr>\n                <td class=\"left-side\">Enable</td>\n                <td><ejs-checkbox id=\"enabled\" :change= \"enabledHandler\" cssClass= \"api\" :checked=\"true\" ></ejs-checkbox></td>\n            </tr>\n            <tr>\n                <td class=\"left-side\">Read only</td>\n                <td><ejs-checkbox id=\"readonly\" :change= \"readonlyHandler\" cssClass= \"api\" :checked=\"false\"></ejs-checkbox></td>\n            </tr>\n            <tr>\n                <td class=\"left-side\"> MaxLength </td>\n                <td><ejs-numerictextbox id=\"maxlength\" format=\"##\" value=\"-1\" :change=\"maxLengthHandler\"></ejs-numerictextbox></td>\n            </tr>\n            <tr>\n                <td class=\"left-side\"> Value </td>\n                <td>\n                    <ejs-textbox id=\"value\" placeholder=\"Enter a value\" :change=\"valueHandler\"></ejs-textbox>\n                </td>\n            </tr>\n            <tr>\n                <td class=\"left-side\">Show clear icon</td>\n                <td><ejs-checkbox id=\"clearicon\" :change= \"clearIconHandler\" cssClass= \"api\" :checked=\"false\"></ejs-checkbox></td>\n            </tr>\n        </table>\n    </div>\n</template>\n\n\n<style>\n    #textarea-api .e-multi-line-input {\n        max-width: 100%;\n    }\n</style>\n\n<style scoped>\n    .content_wrapper div.api-row {\n        margin: 30px 150px;\n    }\n\n    .api-property .left-side {\n        font-size: 14px;\n        padding: 8px;\n    }\n\n    .api-property.property-panel-table div.api {\n        padding-left: 0px;\n    }\n</style>\n\n<script>\n\nimport { TextAreaComponent } from '@syncfusion/ej2-vue-inputs';\nimport { TextBoxComponent } from '@syncfusion/ej2-vue-inputs';\nimport { CheckBoxComponent } from \"@syncfusion/ej2-vue-buttons\";\nimport { NumericTextBoxComponent } from \"@syncfusion/ej2-vue-inputs\";\n\nexport default {\n    data: function () {\n        return {\n            enabled: true,\n            readOnly: false,\n            maxLength: null,\n            rows: null,\n            cols: null,\n            value: \"\",\n            showClearButton: false,\n            enabledHandler: (args) => {\n                this.enabled = args.checked;\n            },\n            readonlyHandler: (args) => {\n                this.readOnly = args.checked;\n            },\n            clearIconHandler: (args) => {\n                this.showClearButton = args.checked;\n            },\n            rowHandler: (args) => {\n                this.rows = args.value;\n            },\n            columnHandler: (args) => {\n                this.cols = args.value;\n            },\n            maxLengthHandler: (args) => {\n                this.maxLength = args.value;\n            },\n            valueHandler: (args) => {\n                this.value = args.value;\n            }\n        }\n    },\n    components: {\n        'ejs-textarea': TextAreaComponent,\n        'ejs-textbox': TextBoxComponent,\n        'ejs-checkbox': CheckBoxComponent,\n        'ejs-numerictextbox': NumericTextBoxComponent\n    }\n};\n</script>\n\n","dependencies":"{\"vue\":\"3.3.4\",\"@syncfusion/ej2-base\":\"*\",\"@syncfusion/ej2-vue-base\":\"*\",\"@syncfusion/ej2-inputs\":\"*\",\"@syncfusion/ej2-vue-inputs\":\"*\",\"@syncfusion/ej2-buttons\":\"*\",\"@syncfusion/ej2-vue-buttons\":\"*\",\"@syncfusion/ej2-vue-charts\":\"*\",\"@syncfusion/ej2-vue-maps\":\"*\",\"@syncfusion/ej2-vue-calendars\":\"*\",\"@syncfusion/ej2-data\":\"*\",\"@syncfusion/ej2-vue-grids\":\"*\",\"@syncfusion/ej2-vue-schedule\":\"*\",\"@syncfusion/ej2-vue-richtexteditor\":\"*\",\"@syncfusion/ej2-vue-navigations\":\"*\",\"@syncfusion/ej2-vue-documenteditor\":\"*\",\"@syncfusion/ej2-vue-splitbuttons\":\"*\",\"@syncfusion/ej2-vue-dropdowns\":\"*\",\"@syncfusion/ej2-vue-pdfviewer\":\"*\",\"@syncfusion/ej2-vue-layouts\":\"*\",\"@syncfusion/ej2-vue-lists\":\"*\"}","src/main.js":"import { createApp } from 'vue'\nimport App from './App.vue'\ncreateApp(App).mount('#app')"}